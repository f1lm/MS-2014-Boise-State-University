<script type="text/javascript">
function safe_for_ads() {
	var path = window.location.pathname;
	if (path === '/'
	 || path === ''
	 || path.match(/^\/stories/)
	 || path.match(/^\/blog/)
	) {
		return true;
	} else {
		return false;
	}
}
function yet_another_tpc() {
	var skin_at_hostname = {
   "" : "awards",
   "books.slashdot.org" : "bookreview",
   "intel.vendors.slashdot.org" : "vendorintel",
   "slashdot.sourceforge.net" : "sfauth",
   "apple.slashdot.org" : "apple",
   "polls.slashdot.org" : "polls",
   "idle.slashdot.org" : "idle",
   "developers.slashdot.org" : "developers",
   "meta.slashdot.org" : "meta",
   "cmdrtaco.slashdot.org" : "cmdrtaco",
   "tech.slashdot.org" : "technology",
   "amd.vendors.slashdot.org" : "vendoramd",
   "thinkgeek.shill.slashdot.org" : "thinkgeek",
   "backslash.slashdot.org" : "backslash",
   "games.slashdot.org" : "games",
   "slashdot.org" : "slashdot",
   "oracle.vendors.slashdot.org" : "vendororacle",
   "hardware.slashdot.org" : "hardware",
   "politics.slashdot.org" : "politics",
   "news.slashdot.org" : "news",
   "apache.slashdot.org" : "apache",
   "vendors.slashdot.org" : "vendors",
   "it.slashdot.org" : "it",
   "bsd.slashdot.org" : "bsd",
   "interviews.slashdot.org" : "interviews",
   "radio.slashdot.org" : "radio",
   "search.slashdot.org" : "search",
   "linux.slashdot.org" : "linux",
   "mobile.slashdot.org" : "mobile",
   "features.slashdot.org" : "features",
   "ask.slashdot.org" : "askslashdot",
   "yro.slashdot.org" : "yro",
   "science.slashdot.org" : "science",
   "entertainment.slashdot.org" : "entertainment"
}
;

	var stories = window.location.pathname.match(/stories\/([^\/#?]+)/);
	var skin;
	if (skin_at_hostname) {
		skin = skin_at_hostname[window.location.hostname];
	}

	if (stories && stories.length) {
		return matches[1];
	} else if (skin) {
		return skin;
	} else {
		return null;
	}
}

dfp_ord=Math.random()*10000000000000000;
dfp_tile = 1;
dfp_page = safe_for_ads() ? 'index' : 'story';
dfp_tpc = yet_another_tpc() || 'slashdot';
</script>

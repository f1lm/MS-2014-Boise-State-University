(function(dust){var htmlRegex=/[<>'"]|&(?!(?:\w+|#(?:\d+|x[\da-f]+));)/i;var encodedText=function(input){if(htmlRegex.test(input)){report(input,"dust-security-ensure_preencode");input=dust.filters.h(input)}return input};var markup=function(input){if(!validateHTML(input))report(input,"dust-security-html_whitelist");return input};var report=function(badValue,violatedDirective){dust.log(violatedDirective+": "+badValue,"WARN")};var createHTMLValidator=function(config){var rNotAllowedTag=new RegExp("\x3c(?!/?(?:"+
config.allowedTags+")(?:[ \t\r\n]+(?:(?:"+config.allowedAttributes+')(?:\x3d"[^\'"\x3c\x3e]*")?|(?:'+config.allowedURLAttributes+')\x3d"(?:(?:'+config.allowedURLSchemes+')(?::|\x26#(?:58|x3a);)|[/.#?]|\x26#(?:35|4[67]|63|x(?:2[3ef]|3f));)[^\'"\x3c\x3e]*"))*[ \t\r\n]*/?\x3e)',"i");return function(html){return!rNotAllowedTag.test(html)}};var validateHTML=createHTMLValidator({allowedTags:"a|abbr|address|area|article|aside|audio|b|bdi|bdo|big|blockquote|br|button|center|cite|code|datalist|dd|del|details|dfn|div|dl|dt|em|fieldset|figcaption|figure|font|footer|h[1-6]|header|hgroup|hr|i|img|ins|kbd|label|legend|li|map|mark|marquee|nav|nobr|ol|p|pre|q|rp|rt|ruby|s|samp|section|small|source|span|strike|strong|sub|sup|table|tbody|td|tfoot|th|thead|time|tr|u|ul|var|video|wbr",
allowedAttributes:"alt|aria-[a-z0-9_\\-]+|border|caption|checked|class|colgroup|color|cols|colspan|controls|coords|data-[a-z0-9_\\-]+|dir|disabled|height|hidden|hreflang|id|label|loop|marginheight|marginwidth|maxlength|method|multiple|name|preload|readonly|rel|required|reversed|role|rows|rowspan|spellcheck|tabindex|target|title|type|usemap|width|clear|headers|ismap|lang|start|datetime|accept|max|min|placeholder|size|step",allowedURLAttributes:"href|src",allowedURLSchemes:"https?|ftp|mailto"});var filters=
{et:encodedText,mk:markup,encodedText:encodedText,markup:markup};for(var key in filters)dust.filters[key]=filters[key]})(dust);
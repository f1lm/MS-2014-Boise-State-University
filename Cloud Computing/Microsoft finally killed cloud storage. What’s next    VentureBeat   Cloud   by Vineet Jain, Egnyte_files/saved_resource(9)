		var moreInformation = $('#profile-cards-header');
		var newvbText = 'More information:';
		moreInformation.text(newvbText);

	}); 
})(jQuery);;
(function() {
	var ajaxurl = window.ajaxurl || '/wp-admin/admin-ajax.php',
		data = window.wpcomVipAnalytics,
		dataQs, percent;

	if ( typeof XMLHttpRequest === 'undefined' ) {
		return;
	}

	if ( ! data ) {
		return;
	}

	percent = ~~data.percentToTrack;
	if ( percent && percent < 100 && ( ~~( ( Math.random() * 100 ) + 1 ) > percent ) ) {
		return;
	}

	dataQs = 'action=wpcom_vip_analytics';

	for ( var key in data ) {
		if ( key === 'percentToTrack' ) {
			continue;
		}
		if ( data.hasOwnProperty( key ) ) {
			dataQs += '&' +
				encodeURIComponent( key ).replace(/%20/g, '+' ) + '=' +
				encodeURIComponent( data[key] ).replace(/%20/g, '+' );
		}
	}

	function sendInfo() {
		var xhr = new XMLHttpRequest();
		xhr.open( 'POST', ajaxurl, true );
		xhr.setRequestHeader( 'Content-type', 'application/x-www-form-urlencoded' );
		xhr.send( dataQs );
	}

	// Delay for some time after the document is ready to ping
	function docReady() {
		setTimeout( function() {
			sendInfo();
		}, 1500 );
	}

	if ( document.readyState === 'complete' ) {
		docReady.apply();
	}
	else if ( document.addEventListener ) {
		document.addEventListener( 'DOMContentLoaded', docReady, false );
	}
	else if ( document.attachEvent ) {
		document.attachEvent( 'onreadystatechange', docReady );
	}
})();
;
var gigya_debug = false;

/**
 * Perform Gigya Socialize interactions.
 */
jQuery( document ).ready(function() {
	gigya.accounts.getAccountInfo({ 'callback' : vb_check_logged_in }); // Check if user is already logged in
});

/**
 * Add Gigya Account Event Handlers
 * @url http://developers.gigya.com/020_Client_API/020_Accounts/accounts.addEventHandlers
 */
gigya.accounts.addEventHandlers({
	onLogin		: vb_gigya_on_login,
	onLogout	: vb_gigya_on_logout
});

/**
 * Declare Login & Logout Links & User Variables
 */
var vb_gigya_user;
var vb_gigya_login_links = '<a href="javascript:;" class="vb-gigya-register">Sign Up</a> | <a href="javascript:;" class="vb-gigya-login">Login</a>';
var vb_gigya_logout_links = '<a href="javascript:;" class="vb-gigya-view-account">View Account</a> | <a href="javascript:;" class="vb-gigya-logout">Logout</a>';
var vb_gigya_set_user_callback;

/**
 * This function determines if a user is logged in or not.
 * @param user
 */
function vb_check_logged_in( user ) {
	if ( gigya_debug ) console.log( 'vb_check_logged_in():', user );
	if ( 'undefined' != typeof user ) {
		if ( 0 === user.errorCode ) {
			if ( gigya_debug ) console.log( 'vb_check_logged_in(): Gigya user IS logged in' );
			set_vb_gigya_user( user );
			jQuery( 'div#container-gigya-links-search' ).prepend( '<div class="account">' + vb_gigya_logout_links + '</div>' );
			declare_vb_gigya_event_handlers_logout();
		} else {
			if ( gigya_debug ) console.log( 'vb_check_logged_in(): Gigya user is NOT logged in' );
			jQuery( 'div#container-gigya-links-search' ).prepend( '<div class="account">' + vb_gigya_login_links + '</div>' );
			declare_vb_gigya_event_handlers_login();
		}
	} else {
		if ( gigya_debug ) console.log( 'vb_check_logged_in(): Ignoring undefined user' );
	}
}

/**
 * This is our Gigya registration wrapper function.  This function can be called by other plugins (like Gigya-Livefyre).
 */
function vb_gigya_register() {
	if ( gigya_debug ) console.log( 'vb_gigya_register(): Gigya Register Started' );
	gigya.accounts.showScreenSet({
		screenSet		: 'VB-Login-Web',
		mobileScreenSet	: 'VB-Login-Mobile',
		startScreen		: 'gigya-register-screen',
		onAfterScreenLoad : vb_gigya_socialprivacy_init
	});
}

/**
 * This is our Gigya login wrapper function.  This function can be called by other plugins (like Gigya-Livefyre).
 */
function vb_gigya_login() {
	if ( gigya_debug ) console.log( 'vb_gigya_login(): Gigya Log In Started' );
	gigya.accounts.showScreenSet({
		screenSet		: 'VB-Login-Web',
		mobileScreenSet	: 'VB-Login-Mobile',
		onAfterScreenLoad : vb_gigya_socialprivacy_init
	});
}

/**
 * This is our Gigya logout wrapper function.  This function can be called by other plugins (like Gigya-Livefyre).
 */
function vb_gigya_logout() {
	if ( gigya_debug ) console.log( 'vb_gigya_logout(): Gigya Log Out Started' );
	gigya.accounts.logout();
}

/**
 * This is our Gigya edit account wrapper function.  This function can be called by other plugins (like Gigya-Livefyre).
 */
function vb_gigya_edit_account() {
	if ( gigya_debug ) console.log( 'vb_gigya_edit_account(): Gigya Edit Account Started' );
	gigya.accounts.showScreenSet({
		screenSet		: 'VB-Profile-Web',
		mobileScreenSet	: 'VB-Profile-Mobile',
		onAfterScreenLoad : vb_gigya_socialprivacy_init
	});
}


/**
 * This is our Gigya SocialPrivacy init function.
 */
function vb_gigya_socialprivacy_init() {
	if ( gigya_debug ) console.log( 'vb_gigya_socialprivacy_init(): Gigya SocialPrivacy Seal loaded' );
	gigya.socialize.showSealUI({
		containerID: "gigya-privacy-seal"
	});
	jQuery( '#gigya-privacy-seal' ).css( 'z-index', '1000000001');
}

/**
 * This function processes Gigya login events.
 * @param eventObj
 */
function vb_gigya_on_login( eventObj ) {
	if ( gigya_debug ) console.log( 'vb_gigya_on_login(): Gigya Login Event - ', eventObj );
	if ( ( 'object' == typeof vb_gigya_settings ) && ( 'function' == typeof vb_gigya_login_ajax ) ) {
		if ( gigya_debug ) console.log( 'vb_gigya_on_login(): Setting vb_gigya_set_user_callback to vb_gigya_login_ajax' );
		vb_gigya_set_user_callback = vb_gigya_login_ajax;
	} else {
		if ( 'function' == typeof vb_gigya_login_listener ) {
			if ( gigya_debug ) console.log( 'vb_gigya_on_login(): Setting vb_gigya_set_user_callback to vb_gigya_login_listener' );
			vb_gigya_set_user_callback = vb_gigya_login_listener;
		} else {
			vb_gigya_refresh();
		}
	}
	gigya.accounts.getAccountInfo({ 'callback' : vb_gigya_login_set_user, 'include' : 'loginIDs, profile, data' });
	vb_gigya_logout_links = 'You have been logged in.'; // Update Gigya Logout Links Variable to prevent attempts to logout immediately after login before page refresh
	jQuery( 'div#container-gigya-links-search div.account' ).html( vb_gigya_logout_links );
	declare_vb_gigya_event_handlers_logout();
}

function vb_gigya_refresh() {
//	document.location.reload();
}

function vb_gigya_login_set_user( user ) {
	if ( 'undefined' != typeof user ) {
		set_vb_gigya_user(user);
	}
}

/**
 * This function processes Gigya logout events.
 */
function vb_gigya_on_logout() {
	if ( gigya_debug ) console.log( 'vb_gigya_on_logout(): Gigya Logout Event' );
	vb_gigya_login_links = 'You have been logged out.'; // Update Gigya Login Links Variable to prevent attempts to login immediately after logout before page refresh
	jQuery( 'div#container-gigya-links-search div.account' ).html( vb_gigya_login_links );
	declare_vb_gigya_event_handlers_login();
	set_vb_gigya_user( undefined, true );
	if ( ( 'object' == typeof vb_gigya_settings ) && ( 'function' == typeof vb_gigya_logout_ajax ) ) {
		if ( gigya_debug ) console.log( 'vb_gigya_on_login(): Calling vb_gigya_login_ajax()' );
		vb_gigya_logout_ajax();
	} else {
		post_callback_logout_function();
	}
}

function set_vb_gigya_user( user ) {
	if ( gigya_debug ) console.log( 'set_vb_gigya_user(): ', user );
	vb_gigya_user = user;
	if ( 'function' == typeof vb_gigya_set_user_callback ) {
		if ( gigya_debug ) console.log( 'set_vb_gigya_user(): Calling vb_gigya_set_user_callback()' );
		vb_gigya_set_user_callback();
		vb_gigya_set_user_callback = undefined;
	}
}

/**
 * This process declares the Gigya Log in event handler(s)
 */
function declare_vb_gigya_event_handlers_login() {
	jQuery( 'a.vb-gigya-register' ).click(function( e ) { // Begin: Process Register Click
		e.preventDefault();
		if ( gigya_debug ) console.log( 'declare_vb_gigya_event_handlers_login(): Gigya Register Started' );
		vb_gigya_register();
	}); // End: Process Register Click
	jQuery( 'a.vb-gigya-login' ).click(function( e ) { // Begin: Process Login Click
		e.preventDefault();
		if ( gigya_debug ) console.log( 'declare_vb_gigya_event_handlers_login(): Gigya Log In Started' );
		vb_gigya_login();
	}); // End: Process Login Click
}

/**
 * This process declares the Gigya Log out event handler(s)
 */
function declare_vb_gigya_event_handlers_logout() {
	jQuery( 'a.vb-gigya-logout' ).click(function( e ) { // Begin: Process Logout Click
		e.preventDefault();
		if ( gigya_debug ) console.log( 'declare_vb_gigya_event_handlers_logout(): Gigya Log Out Started' );
		vb_gigya_logout();
	}); // End: Process Logout Click
	jQuery( 'a.vb-gigya-view-account' ).click(function( e ) { // Begin: Process View Account Click
		e.preventDefault();
		if ( gigya_debug ) console.log( 'declare_vb_gigya_event_handlers_logout(): Gigya View Account Started' );
		vb_gigya_edit_account();
	}); // End: Process View Account Click
}

/**
 * This function performs the AJAX call to sync with WordPress after successful Gigya login.
 */
function vb_gigya_login_ajax() {
	if ( gigya_debug ) console.log( 'vb_gigya_login_ajax(): Making AJAX call to:', vb_gigya_settings.ajax.url );
	if ( 'object' == typeof vb_gigya_settings.ajax ) {
		jQuery.post( vb_gigya_settings.ajax.url, { // Gigya Login AJAX Call
			'action'	: vb_gigya_settings.ajax.actions.login,
			'data'		: vb_gigya_user
		}, function( responseData ) { // Middle of Gigya Login AJAX Call
			if ( gigya_debug ) console.log( 'vb_gigya_login_ajax(): Successful AJAX Call:', responseData );
			post_callback_login_function();
		}, 'json').fail(function() { // Middle of Gigya Login AJAX Call
			if ( gigya_debug ) console.log( 'vb_gigya_login_ajax(): Failed AJAX Call' );
			post_callback_login_function();
		}); // End of Gigya Login AJAX Call
	}
}

function post_callback_login_function() {
	if ( 'function' == typeof vb_gigya_login_listener ) {
		if ( gigya_debug ) console.log( 'vb_gigya_login_ajax(): Executing vb_gigya_login_listener()' );
		vb_gigya_login_listener();
	} else {
		vb_gigya_refresh();
	}
}

/**
 * This function performs the AJAX call to sync with WordPress after successful Gigya logout.
 */
function vb_gigya_logout_ajax() {
	if ( gigya_debug ) console.log( 'vb_gigya_logout_ajax(): Making AJAX call to:', vb_gigya_settings.ajax.url );
	if ( 'object' == typeof vb_gigya_settings.ajax ) {
		jQuery.post( vb_gigya_settings.ajax.url, { // Gigya Login AJAX Call
			'action'	: vb_gigya_settings.ajax.actions.logout,
			'data'		: vb_gigya_user
		}, function( responseData ) { // Middle of Gigya Login AJAX Call
			if ( gigya_debug ) console.log( 'vb_gigya_logout_ajax(): Successful AJAX Call:', responseData );
			post_callback_logout_function();
		}, 'json').fail(function() { // Middle of Gigya Login AJAX Call
			if ( gigya_debug ) console.log( 'vb_gigya_logout_ajax(): Failed AJAX Call' );
			post_callback_logout_function();
		}); // End of Gigya Login AJAX Call
	}
}

function post_callback_logout_function() {
	var listener_completed = false;
	if ( 'function' == typeof vb_gigya_logout_listener ) {
		if ( gigya_debug ) console.log( 'post_callback_logout_function(): Calling vb_gigya_logout_listener()' );
		listener_completed = vb_gigya_logout_listener();
	} else {
		listener_completed = true;
	}
	if ( listener_completed ) {
		vb_gigya_refresh();
	}
};


/* ==================================================

 	V e n t u r e B e a t    D F P    2 0 1 4

   ================================================== */	

(function($) {
	// DFP Work
	  $(function(){ 
		  	/*
		  	 * Insert our ad divs
		  	 */
		  	
		  	// Leaderboard - Desktop/Tablet
			$('.vb-ad-leaderboard').after('<div id="div-gpt-ad-1389653364102-0" class="adslot responsiveAd728" data-sizes="[[728, 90], [970, 31], [970, 45], [970, 66], [970, 90], [970, 120], [970, 250]]" data-dfp="PREPROD_ROS_LB"></div>');
			
		  	// Leaderboard - Mobile
			$('.vb-ad-leaderboard').after('<div id="div-gpt-ad-1403282580091-0" class="adslot responsiveAd320" data-sizes="[320, 50]" data-dfp="PREPROD_ROS_320x50"></div>');
			
			// Med-Rec 2 in the River
			$('.story-river article:eq(2)').after('<article class="advertorial"><div class="ad"><div id="div-gpt-ad-1403283571600-0" class="adslot responsiveAd3002" data-sizes="[[300, 250], [500, 250]]" data-dfp="PREPROD_ROS_MR2"></div></div></article>');
			
			// Med-Rec 2 on all story pages above all LiveFyre comments
			$('#content #comments:eq(0)').prepend('<article class="advertorial"><div class="ad"><div id="div-gpt-ad-1403283571600-0" class="adslot responsiveAd3002" data-sizes="[[300, 250], [500, 250]]" data-dfp="PREPROD_ROS_MR2"></div></div></article>');


			// DFP Background SKins 
			$('#the_footer').append('<div id="div-gpt-ad-1397252889886-0" class="adslot dfp-skin" data-sizes="[1, 1]" data-dfp="Tablet_1x1"></div>');

			var dfpslots=$('body').find('.adslot').filter(':visible'),
			i=0,
			slot= new Array();
			googletag.pubads().collapseEmptyDivs();

			if (dfpslots.length) {
				googletag.cmd.push(function() {
					$(dfpslots).each(function(){
						slot[i] = googletag.defineSlot("/1038375/"+$(this).attr("data-dfp"), JSON.parse($(this).attr("data-sizes")), $(this).attr("id")).addService(googletag.pubads());
					});
					googletag.pubads().enableSingleRequest();
					googletag.enableServices();

					$(dfpslots).each(function(){
						googletag.display($(this).attr("id"));
					});
				});
			}

			var articles = $('.story-river .post');


			var nextSlotId = 1;
			function generateNextSlotName() {
				var id = nextSlotId++;
				return 'adslot' + id;
			}

			// Fetch ads for initial posts, every 10th in the river
			for(var j=1, len=articles.length; j < len; j++ ) {
				if( j % 10 === 0) {
					var slotName = generateNextSlotName();
					$(articles[j+2]).after('<div id="' + slotName + '" style="text-align:center; border-bottom: solid 1px; padding-bottom: 20px; margin-bottom:20px;"></div>');
					googletag.cmd.push(function() {
						var slot = googletag.defineSlot('/1038375/PREPROD_ROS_MR2', [300,250], slotName ).addService(googletag.pubads());
						googletag.display(slotName);
					});
				}
			}

			// Fetch ads for newly added posts loaded from infinite scroll
			$( document.body ).on( 'post-load', function() {
				var length = j + len;
				for(j; j < length; j++) {
					if( j % 10 === 0) {
						var slotName = generateNextSlotName();
						var k = j + 2;
						$('.story-river article').eq(k).after('<div id="' + slotName + '" style="text-align:center; border-bottom: solid 1px; padding-bottom: 20px; margin-bottom:20px;"></div>');
						googletag.cmd.push(function() {
							var slot = googletag.defineSlot('/1038375/PREPROD_ROS_MR2', [300,250], slotName ).addService(googletag.pubads());
							googletag.display(slotName);
							googletag.pubads().refresh([slot]);
						});
					}
				}
			
			});

		});
	})(jQuery); // End $
;

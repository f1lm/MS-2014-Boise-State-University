// vbAlerts = Module Pattern
var vbAlerts = (function($) { 		

var alertsObj = {};

var ajax_list_action = 'vb_hub_list';

	$(function(){ // Wait for DOM Load


alertsObj.loadHub = function() {	
	// create and process the AJAX call to retrieve the hub updates
	$.ajax({
		url: ajax_object.ajax_url,	
		data: {
			action: ajax_list_action
		},
		type: 'GET',
		dataType: 'json',
		success: function(data) {

			var listofAllProp = new Array ('events', 'insight', 'webinars');
			
			var allUpdates = new Array();
			var newUpdates = new Array();

			if (document.cookie.indexOf("last-visit") >= 0) {
			  // They've been here before.
			  var vb_cookie = getCookie('last-visit');
			  var oldUpdates = vb_cookie.split(',');
		    }
		    else { 
		    	// They haven't been here before, so they have seen nothing.
		    	var oldUpdates = new Array();
		    }
			
 			  
			for (var i = 0; i < listofAllProp.length; i++) {
				if(data[listofAllProp[i]] === undefined) {
					continue;
				}
				var propUpdates = data[listofAllProp[i]].updates;
				for (var j = 0; j < propUpdates.length; j++) {
					allUpdates.push( propUpdates[j].id );

					// check if this update is in oldUpdates, if not, add to newUpdates
					if( oldUpdates.indexOf(propUpdates[j].id.toString()) < 0 ){
						newUpdates.push( propUpdates[j]);
						if ($(window).width() < 900) {
						$( '.vb-notification' ).css( 'background-position', '1px -23px');

							$( '.vb-alert' ).on('click', function(){
								$( '.vb-alert' ).tooltip('close');
								
								$(this).toggle(function(){
									$( '.vb-alert' ).css( 'background-position', '1px 0px' );
									}, function() { 
									$( '.vb-alert' ).css( 'background-position', '-27px 0px' );
	
									 });

								})
						//
						} else {
						$( '.vb-notification' ).css( 'background-position', '1px -28px');
						$( '.vb-notification' ).hover(
						       function(){ $(this).css( 'background-position', '-33px -28px'); },
						       function(){ $(this).css( 'background-position', '1px -28px'); }
						)
						$( '.vb-alert' ).on('click', function(){
							$( '.vb-alert' ).tooltip('close');
							$( '.vb-alert' ).css( 'background-position', '1px 0px' ).hover(
									function(){ $(this).css( 'background-position', '-33px 0px'); },
						       		function(){ $(this).css( 'background-position', '1px 0px'); }
								);
						});
						
						}

						$('.alert-' + listofAllProp[i]).append('<a class="new" target="_blank" href="' + propUpdates[j].clickthrough + '">' + '<img class="new-update" src="https://venturebeat.files.wordpress.com/2014/07/icons_plus.png" /> ' + propUpdates[j].title + '<br />' + propUpdates[j].subtitle + '</a>' + '<br /><br />');
					} else {
						$('.alert-' + listofAllProp[i]).append('<a target="_blank" href="' + propUpdates[j].clickthrough + '">' + propUpdates[j].title + '<br />' + propUpdates[j].subtitle + '</a>' + '<br /><br />');
					}

					
				};
				
			};

			// Hide New Update Img After Clicking Link
			$( '.vb-alerts a' ).click(function( ) {
			  $( this ).children( 'img' ).hide();
			});

			$(window).resize(calculation);

			function calculation() {
				var alarm_position_left = $( '.alerts-position' ).offset().left;
				$('.ui-tooltip').css('left', alarm_position_left + 47);
		    }

			function getCookie(cname) {
				var name = cname + "=";
			    var ca = document.cookie.split(';');
			    for(var i=0; i<ca.length; i++) {
			        var c = ca[i];
			        while (c.charAt(0)==' ') c = c.substring(1);
			        if (c.indexOf(name) != -1) return c.substring(name.length,c.length);
			    }
			    return "";
			}

			// if there are new updates, update the cookie with the allUpdates array
			if(newUpdates.length > 0) {
			  	document.cookie = 'last-visit=' + allUpdates;
				$( '.vb-alert' ).tooltip({ content: '<a target="_blank" href="' + newUpdates[0].clickthrough + '">' + newUpdates[0].title + ' - ' + newUpdates[0].subtitle + '</a><a class="alerts-x" style="position:absolute;top:0;right:4px;" href="">x</a>' });
				$( '.vb-alert' ).tooltip({ position: { my: "left center" } });
				$( '.vb-alert' ).tooltip('open');
				$( '.alerts-x' ).on( 'click', function(e){
					e.preventDefault();
					$( '.vb-alert').tooltip('close'); 
				});

			}

		}
	});
	
};
		alertsObj.loadHub();


	// Build Alerts Drop
	alertsObj.alertsDrop = function(iconClick) {
		var	alertsContainer = $('.alert-container');
		var alertsArrow = $('img#arrow-up');
			$(iconClick).click(function(e){

				e.preventDefault();
				if (alertsContainer.hasClass('opened')) {
					$(alertsContainer, alertsArrow).fadeIn(325).toggleClass('closed opened');  
				    alertsArrow.fadeIn(325);
				  } else {
					$(alertsContainer, alertsArrow).fadeOut(325).toggleClass('closed opened');
					alertsArrow.fadeOut(325);
			    }


			$('body').on('click', function(event) {
				if( $(event.target).parents('.alert-container').size() > 0) {

				} else if( $(event.target).hasClass('vb-notification') ) {

				} else {
					alertsContainer.fadeOut(325).toggleClass('closed opened');
					alertsArrow.fadeOut(325);
				}
			});
			});


	}



	// Invoke Alerts Drop  	
	alertsObj.alertsDrop('a.vb-notification');


	}); // End Doc Ready

	return alertsObj;
})(jQuery); // End $

;

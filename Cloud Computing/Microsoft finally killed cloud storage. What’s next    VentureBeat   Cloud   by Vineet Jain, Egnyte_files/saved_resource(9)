jQuery( document ).ready( function() {
	var body = jQuery( '.post-content' );
	var channel = jQuery( '.channel-label' ).text();
	var tweet_length;
	var via;
	var trunc;
	var full_tweet;
	body.find( 'q' ).each( function() {
		var quote = jQuery( this ).text();

		if ( channel === 'GamesBeat' ) {
			via = ' - via @gamesbeat'
			full_tweet = quote + via;
			tweet_length = full_tweet.length + shortlink.length + 1
		} else {
			via = ' - via @venturebeat';
			full_tweet = quote + via;
			tweet_length = full_tweet.length + shortlink.length + 1
		}

		if( tweet_length < 140 ) {
			quote = quote + via;
		} else {
			trunc = tweet_length - 140;
			quote = quote.slice(0,-(trunc+3)) + '...' + via;
		}

		jQuery( this ).append( '<span class="tweet-quote"></span>' );

		jQuery( this ).mouseenter( function() {
			jQuery( this ).css( "background-color", "#4099ff" );
			jQuery( this ).css( "color", "white" );
			jQuery( this ).find( '.tweet-quote' ).addClass( "hover-tweet-quote" );
		});

		jQuery( this ).mouseleave( function() {
			jQuery( this ).css( "background-color", "transparent" );
			jQuery( this ).css( "color", "rgb(35,31,32)" );
			jQuery( this ).find( '.tweet-quote' ).removeClass( "hover-tweet-quote" );
		});

		jQuery( this ).wrapInner( '<a target="_blank" class="tweet-link" href="https://twitter.com/intent/tweet?url=' + encodeURIComponent( shortlink ) + '&text=' + encodeURIComponent( quote ) + '"></a>' );
		

	});
});;

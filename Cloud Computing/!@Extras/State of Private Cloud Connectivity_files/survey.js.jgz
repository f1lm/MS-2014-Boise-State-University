	adjustColor: function(parent){
		$(parent).find("tbody tr:not(.sg-hide)").each(function(i, el) {
			$(el).removeClass('sg-even-row').removeClass('sg-odd-row')
			if (i % 2 === 0) { 
				$(el).addClass('sg-odd-row')
			}
			else {
				$(el).addClass('sg-even-row')
			}
		});
	},

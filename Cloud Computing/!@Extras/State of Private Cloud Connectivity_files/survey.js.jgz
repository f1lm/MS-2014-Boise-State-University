						break;		
					case "dialog":
						bubble = new Dialog_Bubble(); 
						break;	
					case "alert":
						bubble = new Alert_Bubble(); 
						break;	
					case "modal":
						bubble = new Modal_Bubble(); 
						break;	
					case "lightbox":

/** 
* This function initializes the events for a survey page based on the currently loaded object
* this includes:  
*		Show/Hide
*		Accessibility Helpers
*		Validation
*		On-Page Merge Codes
*		Calculations, etc. 
**/
SGSurvey.prototype.InitPage = function (page_id, admin_view) {
	var self = this;
	
	this.page_id = page_id;
	
	this.checkForwardOnly();

	currency_glyph_global = this.surveyObject.currency_glyph;
	
	var quest, 
		len = this.surveyObject.questions.length,
		types = [];
			
	var oArgs = {
		sgSurvey: this,
		surveyObject: this.surveyObject,
		pageId: this.page_id
	}
	
	// init sgapi survey class
	SGAPI.survey.init(oArgs);

	// Setup dynamic logic and handlers for each question type
	//for (var q in this.surveyObject.questions)	{
	$.each(this.surveyObject.questions, function(id, quest){	
		//quest = this.surveyObject.questions[q];
		if (!quest || quest.properties.disabled || quest.type == "PAGE_TIMER") {
			return;
		}
		self.InitializeQuestion(quest,admin_view);	
	
		// NEW CHECK
		if (quest.properties.stars || quest.properties.stars_review) {
			types["stars"] = 1;
		}
		else if(quest.properties.thumbs){
			types['thumbs'] = 1;
		}
	})
	
	// question inits for new survey class
	if (types["stars"]) {
		SGAPI.survey.initStarRanking();
	}
	else if (types["thumbs"]) { //added this for thumbs -- Matt Null
		SGAPI.survey.initThumbsQuestion();
	}

	
	//setup page timer (doesn't init with other questions, could be from a previous page)
	if(this.surveyObject.pageTimer){
		var timerOpts = this.surveyObject.pageTimerOpts;
		var page_error = false;
		$.each(this.surveyObject.pages, function(id, page){
		    page_error = page.haserrors;
		    return false;
		})
		
		var opts = {
			type 	    : this.surveyObject.pageTimer,
			limit 	    : timerOpts.limit,
			redirect    : timerOpts.redirect_sku,
			autoSub	    : timerOpts.auto_submit,
			hideNext    : timerOpts.hide_next,
			spanTime    : this.surveyObject.pageTimerSpan,
			pageTime    : this.surveyObject.pageTime,
			repeat		: timerOpts.timer_repeat,
			requiredAction	: timerOpts.required_action,
			error		: page_error
		};
		var pageTimer = new PageTimer(opts);
	}
	
	// set up display for show/hide triggers
	SGAPI.survey.checkQuestionDependents();
	
	if(typeof admin_view == 'undefined')
	{
		//setTimeout("sgTimer(" + this.surveyObject.id + ")" , this.timer_pulse);
		this.isEditor = false;
	}
	else
	{
		this.isEditor = true;
	}

	// Matt Null added this bc this was breaking in polls (the class was not being included)
	try{
		cal = Calendar.setup({
			fdow     : 0,
			onSelect   : function(cal) {cal.hide(); }
		});
	}
	catch(e) {}
	
	this.InitValidationHooks();
	
	// Setup survey interactions
	if (this.interVirtualPage && !$SG('body').hasClass('sg-mobile-offline')) { // Virtual paging
		this.buildMobilizer();
	}
	else if (this.interCollapsed) { // Collapsed answer options
		this.buildCollapsedInter();
	}
	else if (this.interBlurred) { // Blurred questions
		this.buildBlurredInter();
	}
}

// Survey collapse interaction
SGSurvey.prototype.buildCollapsedInter = function () {
	var $set = $SG('.sg-question-set')
	,   $quests = $set.children('.sg-question')
	,   openClass = 'sg-collapse-open'
	
	,   changed = function () {
		var $quest = $SG(this).closest('.sg-question.sg-type-group');
		// Not a custom group
		$quest.length || ($quest = $SG(this).closest('.sg-question'));
		$quest.next('.sg-question').addClass(openClass);
	};
	
	$quests
	.not('.sg-type-slider')
	.on('change', ':input', changed);
	$quests
	.filter('.sg-type-slider')
	.on('sgquestchange', ':input', changed);
	
	$quests.first().addClass(openClass);
};

// Survey blur interaction
SGSurvey.prototype.buildBlurredInter = function () {
	var $set = $SG('.sg-question-set')
	,   notQuests = ".sg-type-group, .sg-type-slider"
	,   $quests = $set.children('.sg-question')
	,   openClass = 'sg-blur-focused'
	
	,   changed = function () {
		var $quest = $SG(this).closest('.sg-question.sg-type-group');
		// Not a custom group
		$quest.length || ($quest = $SG(this).closest('.sg-question'));
		
		if(!$quest.hasClass('sg-blur-focused'))
			return
		if($quest.hasClass('sg-type-checkbox') && !$SG(this).parent('li').hasClass('sg-last-li')){ //don't scroll if checkbox
			return
		}
		// Scroll to next question
		var $next = $quest.nextAll('.sg-question:visible').first();
		$next.length && $SG('body').scrollTo( $next, {offset: { top : -50 }, duration : 600 } );
	};
	
	$quests
	.not(notQuests)
	.filter(':visible')
	.on('change', ':input', changed);
	
	$quests//trying to fix show hide questions
	.not(notQuests)
	.filter(':not(:visible)')
	.on('showhidden', function(){
		//need to revisit this
		//$(this).off('showhidden').on('change', ':input', changed);
	})
	
	$quests
	.filter('.sg-type-slider')
	.on('sgquestchange', ':input', changed);
	
	$quests
	.on('scrollSpy:enter', function() {
		$SG(this).addClass(openClass);
	})
	.on('scrollSpy:exit', function() {
		$SG(this).removeClass(openClass);
	}).first().addClass(openClass);
	
	$quests.scrollSpy({
		offsetTop : 50
		, offsetBottom : -50
	});
};

SGSurvey.prototype.buildMobilizer = function () {
	// Thank you page or terminal page
	if (!this.interVirtualPage) {
		return;
	}
	
	var $set = $SG('.sg-question-set')
		, $quests = $set.children('.sg-question')
		, $forwardSubmit = $SG('#sg_NextButton, #sg_SubmitButton')
		, $backSubmit = $SG('#sg_BackButton')
		, $btnBar = $SG('.sg-button-bar')
		, that = this
		, hasTimer = this.surveyObject.pages['p'+this.surveyObject.currentpage].options.HasTimer
		, hideNext = false; // this.surveyObject.pageTimerOpts.hide_next
		
	// Navigating to prev page (page direction = -2) use last, otherwise first
	var fromNext = this.surveyObject.page_direction == '-2';
	if($SG('body').hasClass('sg-mobile-offline')){
		var $next = $quests[fromNext ? 'last' : 'first'](':visible:not(.sg-hide)').addClass('sg-page-shown');
	}
	else{
		var $next = $quests.filter(':visible:not(.sg-hide)')[fromNext ? 'last' : 'first']().addClass('sg-page-shown');
	}
	if($('.sg-error-message.sg-error-display:not(.mobile-error-msg)').length){//if server side validation error
		$next = $($SG('.sg-question-set').find('.sg-errors')[0])
		if(!$next.length){
			$next = $($SG('.sg-question-set').find('.sg-warning')[0]) //for soft required
		}
		if($next.closest('.sg-group-item').length){ //errors in groups
			$next = $next.closest('.sg-type-group')
		}
	}
	
	this.adjustMobileTableRows($next);
	$(function() {
		that.mobileMoveInPage($next, !fromNext);
	});
	
	// Next button
	var $nextBtn = $SG('<input type="button" class="sg-button sg-next-button sg-mobile-next" value="&#xf0da;"/>');
	if ($forwardSubmit.length)
		$nextBtn = $nextBtn.insertAfter($forwardSubmit);
	else
		$nextBtn = $nextBtn.prependTo($btnBar);
		
	$nextBtn.click(function(){
		that.checkValidation() && that.goMobilePage();
	});
	
	$forwardSubmit.click(function(e){
		if (!that.checkValidation() || !that.eventCanPage()) {
			e.preventDefault()
			return;
		}
		var $current = $SG('.sg-page-current');
		$current.length && that.mobileMoveOutPage($current, true);
	})
	
	$backSubmit.click(function(){
		var $current = $SG('.sg-page-current');
		$current.length && that.mobileMoveOutPage($current, false);
	});
	
	// Back button
	var $backBtn = $SG('<input type="button" class="sg-button sg-back-button sg-mobile-back" value="&#xf0d9;"/>');
	if ($backSubmit.length)
		$backBtn = $backBtn.insertAfter($backSubmit);
	else
		$backBtn = $backBtn.appendTo($btnBar);

	$backBtn.click(function(){
		that.goMobilePage(-1);
	});
	
	function autoNextPage () {
		// Wait for logic events to trigger first
		setTimeout(function(){
			if ($('.sg-mobile-next').is(':visible')) {
				that.goMobilePage()
			}
			else if (!hasTimer || hideNext != 'yes') {
				$forwardSubmit.click();
			}
		}, 200);
	}
	
	function autoBackPage () {
		$backBtn.is(':visible') ? that.goMobilePage(-1) : $backSubmit.click();
	}
	
	$SG('body').on('swipeleft', function(e){
		if ($SG(e.target).closest('.sg-slider').length || 
			$SG(e.target).closest('.sg-type-signature').length ||
			$SG(e.target).closest('.sg-type-rank-dragdrop').length) 
			return;
		if (!that.checkValidation()) {
			e.preventDefault()
			return;
		}
		autoNextPage();
	});
	
	$SG('body').on('swiperight', function(e){
		if ($SG(e.target).closest('.sg-slider').length || 
			$SG(e.target).closest('.sg-type-signature').length ||
			$SG(e.target).closest('.sg-type-rank-dragdrop').length) 
			return;
		autoBackPage();
	});
	
	// Make radio buttons trigger next page
	$SG('.sg-type-radio :radio, .sg-type-table-radio :radio, .sg-type-nps :radio').on('click.mobilizer', function() {
		var $q = $(this).closest('.sg-question.sg-page-current')
			, $li = $(this).closest('li');
			
		// Don't move page if an other option is selected or the 
		// question is not on the current page
		// or if there is a comment
		if ($li.hasClass('sg-other-li') || !$q.length || $q.find('div.sg-comment').length || $q.hasClass('sg-type-group')) {
			return;
		}

		autoNextPage();
	});
	
	// Make radio buttons trigger next page
	$SG('.sg-type-menu select').on('change.mobilizer', function() {
		var $q = $(this).closest('.sg-question.sg-page-current')
		if ($(this).val() != 'NoAnswer' && $(this).attr('multiple') != 'multiple' && !$q.find('div.sg-comment').length && !$q.hasClass('sg-type-group'))
			autoNextPage();
	});
	
	//make the go button go
	$SG('input[type="text"].sg-input,input[type="password"].sg-input,input[type="date"].sg-input,input[type="number"].sg-input,input[type="email"].sg-input').on('keydown',function(e){
    	 if(e.keyCode==13)
          {
              autoNextPage();
              e.preventDefault();
              return false;
          }   
	});
	this.adjustMobilePageControls();
};

SGSurvey.prototype.eventCanPage = function (d) {
	var evt = $.Event('Mobilizer.page' + (d == -1 ? 'prev' : 'next'));
	$('body').trigger(evt, [$SG('.sg-page-current')]);
	return !evt.isDefaultPrevented();
};

SGSurvey.prototype.adjustMobilePageControls = function () {
	var $current = $SG('.sg-page-current')
		, hasNext = $current.nextAll('.sg-question:visible').length
		, hasPrev = $current.prevAll('.sg-question:visible').length
		, hasTimer = this.surveyObject.pages['p'+this.surveyObject.currentpage].options.HasTimer;
		var hideNext = false;
		var timerComplete = false;

		if(hasTimer){
			hideNext = this.surveyObject.pageTimerOpts.hide_next;
			timerComplete = this.surveyObject.pageTime >= this.surveyObject.pageTimerOpts.limit;
		}
		
	if($('.sg-error-message.sg-error-display:not(.mobile-error-msg)').length){//if server side validation error
		hasNext = $current.nextAll('.sg-question.sg-errors:visible').length || $current.nextAll('.sg-question.sg-warning:visible').length
		hasPrev = $current.prevAll('.sg-question.sg-errors:visible').length || $current.prevAll('.sg-question.sg-warning:visible').length
	}
	$SG('#sg_NextButton, #sg_SubmitButton, #sg_BackButton, .sg-mobile-next, .sg-mobile-back').hide();
	// Button visibility also depends on table rows
	if ($current.hasClass('sg-type-table') && !$current.hasClass('sg-type-rank-table')) {
		var otherClass    = 'sg-mobile-table-row'
		, currentClass    = 'sg-mobile-table-row-current'
		, notFilter       = '.sg-hide'
		, forFilter       = '.' + otherClass
		, currentFilter   = '.' + currentClass
		, $rows           = $SG('.sg-table tbody tr', $current).not(notFilter)
		, $vis            = $rows.filter(currentFilter);
			
		hasNext = hasNext || $rows[$rows.index($vis[0]) + 1];
		hasPrev = hasPrev || $vis.prevAll(forFilter).length;
	}
	if (hasNext)
		$SG('.sg-mobile-next').show();
	else{
		if(!hasTimer || (hideNext != 'yes' || timerComplete || this.timerComplete))
			$SG('#sg_NextButton, #sg_SubmitButton').show();
	}
	
	if (hasPrev)
		$SG('.sg-mobile-back').show();
	else
		$SG('#sg_BackButton').show();
};

SGSurvey.prototype.checkValidation = function () {
	var that = this
	var qid = $SG('.sg-page-current').attr('id') && $SG('.sg-page-current').attr('id').split('-')[3] * 1;
	var question_obj = this.surveyObject.questions[qid]
	var selector = 'sgE-' + this.surveyObject.id + '-' + this.surveyObject.currentpage + '-' + qid;
	var question = $('[name="'+selector+'"]');
	var question_element = question[0]
	var validation_ok = true;
	var warning_shown = false
	var hasTimer = this.surveyObject.pages['p'+this.surveyObject.currentpage].options.HasTimer;
	
	//is it answered?
	var type = question_obj.type;
	if(type == 'IMAGE_SELECT' && question_obj.properties.image_select_type == 'multi-select')
		type = 'CHECKBOX'
	var answered = that.checkAnswered(type, selector, question_obj.properties)
	//check required
	if(question_obj.properties.required && !answered){
		validation_ok = false;
		that.ShowWarning(this.messages.required)
		warning_shown = true
	}
	//check for required table rows
	if(type == 'TABLE' && $('#' + selector + '-box tbody tr:visible').hasClass('sg-required-row')){
		validation_ok = that.tableRowValidation(selector, question_obj.properties.table_type)
		if(!validation_ok){
			if(!warning_shown){
				that.ShowWarning(this.messages.required)
				warning_shown = true
			}
		} 
	}
	//if answered, check if there is a required comment or other
	if(answered){
		//let's see if there is a required comment
		var comment_box = $SG('.sg-page-current').find('div.sg-comment');
		if(comment_box.length && comment_box.hasClass('sg-required') && comment_box.find('textarea').val() == ''){
			validation_ok = false;
			if(!warning_shown){
				that.ShowWarning(this.messages.required)
				warning_shown = true
			}
		}
		
		if(type == 'RADIO'){
			//do we have a required other?
			var checked_li = $("[name |= " + selector +"]:checked").closest('li');
			if(checked_li.hasClass('sg-other-li')){
				if(checked_li.find('.sg-input-text-other').hasClass('sg-other-required')){
					var other_val = checked_li.find('.sg-input-text-other').val()
					if(other_val == ''){
						validation_ok = false;
						if(!warning_shown){
							that.ShowWarning(this.messages.required)
							warning_shown = true
						}
					}
				}
			}
		}
		if(type == 'CHECKBOX'){
			//do we have a required other?
			var checked_req_li = $("[name |= " + selector +"]:checked").closest('li').find('.sg-input-text-other.sg-other-required');
			checked_req_li.each(function(){
				if($(this).val() == ''){
					validation_ok = false;
					if(!warning_shown){
						that.ShowWarning(that.messages.required)
						warning_shown = true
					}
				}
			})
		}

		if(type == 'CHECKBOX' && question_obj.properties.minimum_response && question_obj.properties.minimum_response > 0){
			var selected = 0;
			$("[name |= " + selector +"]:checked").each(function() {
				if($(this).val().length)
					selected++;
			});
			if(selected < question_obj.properties.minimum_response){
				validation_ok = false;
				error_msg = this.messages.minimum_response.replace(":min", question_obj.properties.minimum_response).replace(":value", selected)
				if(!warning_shown){
					that.ShowWarning(error_msg)
					warning_shown = true
				}
			}
		}
	}
	//check textbox validation
	if(type == 'TEXTBOX'){
		if(question_obj.properties.map_key == 'date' && (question_obj.properties.mobile_min_date || question_obj.properties.mobile_max_date)){
			var date_parts = $(question_element).val().split('-')
			var date = new Date(date_parts[0],date_parts[1] - 1,date_parts[2]);
			if(question_obj.properties.mobile_min_date != '' && question_obj.properties.mobile_min_date != undefined){
				var min_parts = question_obj.properties.mobile_min_date.split('-')
				var min = new Date(min_parts[0],min_parts[1] - 1,min_parts[2]);
				if(date < min){
					validation_ok = false;
					if(!warning_shown){
						that.ShowWarning(this.messages.invalid_min_date.replace(":min", question_obj.properties.mobile_min_date))
						warning_shown = true
					}
				}
			}
			if(question_obj.properties.mobile_max_date != '' && question_obj.properties.mobile_max_date != undefined){
				var max_parts = question_obj.properties.mobile_max_date.split('-')
				var max = new Date(max_parts[0],max_parts[1] - 1,max_parts[2]);
				if(date > max){
					validation_ok = false;
					if(!warning_shown){
						that.ShowWarning(this.messages.invalid_max_date.replace(":max", question_obj.properties.mobile_max_date))
						warning_shown = true
					}
				}
			}
		}
		if(!question_element.checkValidity()){
			validation_ok = false;
			if(question_obj.properties.map_key != 'date')
				$('#' + selector + '-element').addClass('sg-validation-error');
			if(question_obj.properties.map_key == 'email'){
				if(!warning_shown){
					that.ShowWarning(this.messages.invalid_email.replace(":value", $(question_element).val()))
					warning_shown = true
				}
			}
			if(question_obj.properties.map_key == 'number'){
				if(question_obj.properties.only_positive_num){
					if(!warning_shown){
						that.ShowWarning(this.messages.must_be_positive_whole_number.replace(":value", $(question_element).val()))
						warning_shown = true
					}
				}
				else if(question_obj.properties.only_whole_num){
					if(!warning_shown){
						that.ShowWarning(this.messages.must_be_whole_number.replace(":value", $(question_element).val()))
						warning_shown = true
					}
				}
				else{
					if(!warning_shown){
						that.ShowWarning(this.messages.invalid_number.replace(":value", $(question_element).val()))
						warning_shown = true
					}
				}
			}
		}
		else{
			$('#' + selector + '-element').removeClass('sg-validation-error');
		}
	}
		
	if(question){
		question.trigger('blur')
		if($SG('.sg-page-current .sg-control-text').hasClass('sg-validation-fail') || question.hasClass('sg-validation-error')){
			if(question_obj.properties.map_key == 'percent'){
				validation_ok = false;
				if(!warning_shown){
					that.ShowWarning(this.messages.invalid_percent.replace(":value", $(question_element).val()))
					warning_shown = true
				}
			}
		}
	}
	
	validation_ok = hasTimer && type != 'IDENTIFY' ? true : validation_ok;
	
	return validation_ok;
};

SGSurvey.prototype.checkAnswered = function (type, selector, properties) {
	var that = this;
	var answered = true;
	switch(type){
		case 'IMAGE_SELECT' :
		case 'RADIO' :
		case 'NPS' :
			var select = "[name = " + selector +"]"
			if( $(select+':checked').val() == undefined) answered = false;
			break;
		case 'ESSAY' :
		case 'TEXTBOX' :
			if($('#' + selector + '-element').val() == '') answered = false;
			break;
		case 'MENU':
			if($('#' + selector + '-element').val() == 'NoAnswer') answered = false;
			break;
		case 'SLIDER' :
		case 'MULTI_SLIDER' :
		case 'MULTI_TEXTBOX' :
		case 'CONT_SUM' :
		case 'CHECKBOX' :
		case 'RANK' :
			if(type == 'CHECKBOX' || (type == 'RANK' && this.surveyObject.ismobile == 'true')) select =  "[name |= " + selector +"]:checked"
			if(type == 'MULTI_TEXTBOX' || type == 'CONT_SUM'|| (type == 'RANK' && this.surveyObject.ismobile != 'true')) select =  "[name |= " + selector +"]:text"
			if(type == 'SLIDER' || type == 'MULTI_SLIDER') select = '#' + selector + '-box .sg-slider-hidden-value :hidden'
			
			var allSelected = []
			$(select).each(function() {
				if($(this).val().length)
					allSelected.push($(this).val());
			});
			if(allSelected.length === 0) answered = false;
			break;
		case 'TABLE' :
			answered = that.tableRowValidation(selector, properties.table_type)
			break;
		case 'FILE' :
			if($("#sg-file-desc-" + selector).find('li:not(.sg-prototype-li)').length == 0) answered = false;
			break;
		case 'GROUP' :
			$('#' + selector + '-box').find('.sg-group-item .sg-question').each(function(){
				var qid = $(this).attr('id').split('-')[3] * 1
				var question_obj = that.surveyObject.questions[qid]
				var selector = 'sgE-' + that.surveyObject.id + '-' + that.surveyObject.currentpage + '-' + qid;
				answered = that.checkAnswered(question_obj.type, selector, question_obj.properties)
			})
			break;
		case 'MATRIX' :
			var allSelected = []
			$.each($('.sg-page-current').find(':radio:checked, :text, textarea'), function(){
				if($(this).val().length)
					allSelected.push($(this).val());
			})
			if($('.sg-page-current .sg-mobile-table-row-current').hasClass('sg-last-row') && allSelected.length === 0)
				answered = false;
			break;
		case 'MAXDIFF' :
			var allSelected = []
			var num_items = 0
			$('#' + selector + '-box').find('.sg-maxdiff-set:visible select').each(function() {
				if($(this).val().length && $(this).val() != 'NoAnswer')
					allSelected.push($(this).val());
				num_items++
			});
			if(allSelected.length != num_items) answered = false;
			break;
	}
	return answered;
}

SGSurvey.prototype.tableRowValidation = function (selector, table_type) {
	if (table_type == 'SEMANTIC') {
		var $hiddens = $SG('#' + selector + '-box .sg-slider-row .sg-input-hidden')
		// # answered > 0
		return ($hiddens.length - $hiddens.filter('[value=""]').length) > 0;
	}
	var t_q_type_select;
	if(table_type == 'MENU' || table_type == 'MENU_MATRIX') t_q_type_select = 'select';
	else if(table_type == 'MULTI_TEXTBOX') t_q_type_select = 'input:text';
	else if(table_type == 'TEXTBOX') t_q_type_select = 'input:text';
	else t_q_type_select = 'input:checked';
	var allSelected = []
	$('#' + selector + '-box tbody tr:visible ' + t_q_type_select).each(function() {
		if(table_type == 'MENU_MATRIX'){
			if($(this).val().length && $(this).val() != 'NoAnswer')
				allSelected.push($(this).val());
		}
		else if(table_type == 'MENU'){
			if($(this).val() == 'NoAnswer'){
				allSelected = [];
				return false;
			}
			else{
				allSelected.push($(this).val());
			}
		}
		else{
			if($(this).val().length)
				allSelected.push($(this).val());
		}
	});
	if(allSelected.length === 0){
		return false;
	}
	else
		return true;
}

SGSurvey.prototype.ShowWarning = function (msg) {
	if(!$('.mobile-error-msg').length){
		var required = '<div class="mobile-error-msg sg-error-message sg-error-display">'+ msg +'</div>'
		$(".sg-content").prepend(required)
	}
	else{
		$('.mobile-error-msg').html(msg)
	}
	$('.mobile-error-msg').slideDown(500, function(){
		setTimeout(function(){
			$('.mobile-error-msg').slideUp(500);
		},3000);
	});
};

SGSurvey.prototype.adjustMobileTableRows = function ($q, pos) {
	$q = $SG($q);
	pos = pos || 'first';

	if ($q.hasClass('sg-type-table')) {
		var $rows = $SG('.sg-table tbody tr:not(.sg-hide)', $q), otherClass = 'sg-mobile-table-row', currentClass = 'sg-mobile-table-row-current';
		$rows.removeClass(currentClass).addClass(otherClass);
		$rows[pos]().removeClass(otherClass).addClass(currentClass);
	}
	if($q.hasClass('sg-type-rank-table')){
		this.bindRankTable($q);
		this.bindClearRank($q);
	}
};
SGSurvey.prototype.bindRankTable = function($q){
	var qid = $q.attr('id').split('-')[3] * 1;
	var question_obj = this.surveyObject.questions[qid]
	this.rankIndex = parseInt($('.sg-rank-counter', $q).val());
	var that = this,
	currentRank = 0;
	$.each($q.find('tbody tr'), function (mainIndex){
		var $row = $(this);
		$row.find('th input.sg-rank-trigger').off().on('change', function(){
			var ranks = $row.find('.sg-rank-control-label')
				, parent = $(this).closest('tr');
			if(!$(this).is(':checked')){
				$(parent).addClass('sg-empty-row');
				$.each($(ranks), function(){
					var idSelector =  $(this).attr('for');
					$(this).addClass('hide');
					if($('#' + idSelector).is(":checked")){
						currentRank = parseInt($('#' + idSelector).val());
					}
					$('#' + idSelector).attr('checked',false);
				});
				if(currentRank < that.rankIndex){
					$.each($q.find('tbody tr'), function(index){
						var elem = $(this).find('td label').not('.hide')
							, newRanks = $(this).find('.sg-rank-control-label');
						if (mainIndex != index 
							&& elem.length > 0
							&& parseInt($(elem).data('rank_index') + 1) > currentRank){
								var rankIndex = parseInt($(elem).data('rank_index')) - 1;
								$(elem).addClass('hide');
								$(elem).attr('checked',false);
								$(newRanks[rankIndex]).removeClass('hide');
								$('#' + $(newRanks[rankIndex]).attr('for')).attr('checked',true);
						}
					})
				}
				that.rankIndex--;
				$.each($q.find('.sg-empty-row'), function(){
					$(this).removeClass('sg-rank-row-disabled')
					$(this).find('input.sg-rank-trigger').attr('disabled', false)
				})
			}
			else{
				$(parent).removeClass('sg-empty-row');
				$(ranks[that.rankIndex]).removeClass('hide');
				$('#' + $(ranks[that.rankIndex]).attr('for')).attr('checked',true);
				that.rankIndex++;
				if(that.rankIndex == question_obj.properties.xnumber){
					$.each($q.find('.sg-empty-row'), function(){
						$(this).addClass('sg-rank-row-disabled')
						$(this).find('input.sg-rank-trigger').attr('disabled', 'disabled')
					})
				}
				
			}
			$('.sg-rank-counter', $q).val(that.rankIndex);
		});
	});
}
SGSurvey.prototype.bindClearRank = function($q) {
	var clear = $('.sg-clear-rank');
	$(clear).on('click', function() {
		$.each($q.find('tbody tr'), function(){
			if($(this).find('input').is(':checked')){
				$(this).find('input').attr('checked', false);
				$(this).find('th input.sg-rank-trigger').trigger('change');
			}
		});
	})
}
SGSurvey.prototype.updateProgressBar = function () {
	var $progress_bar_text = $SG('.sg-progress-bar-text'),
	$progress_bar_inner = $SG('.sg-progress-bar-inner'),
	progress_text = $progress_bar_text.text(),
	q_count = 0;
	
	if (Object.keys)
		q_count = Object.keys(this.surveyObject.questions).length;
	else
		for (var k in this.surveyObject.questions) q_count++;
	
	if (this.actualProgress === undefined) {
		this.actualProgress = parseInt(progress_text.slice(0,-1));
	}
	
	// Round to nearest 10th
	this.actualProgress += Math.round((this.surveyObject.minstep / q_count) * 10) / 10;
	
	$progress_bar_text.text(Math.floor(this.actualProgress) + '%');
	$progress_bar_inner.css('width', this.actualProgress + '%');
};

SGSurvey.prototype.goMobileTableRow = function ($current, goPrev) {
	var otherClass     = 'sg-mobile-table-row'
	, currentClass     = 'sg-mobile-table-row-current'
	, notFilter        = '.sg-hide'
	, forFilter        = '.' + otherClass
	, currentFilter    = '.' + currentClass
	, $rows            = $SG('.sg-table tbody tr', $current).not(notFilter)
	, $vis             = $rows.filter(currentFilter)
	, $sel             = $($rows[$rows.index($vis[0]) + (goPrev ? -1 : 1)])
	, $navBtns         = $SG('.sg-mobile-next, .sg-mobile-back')
	, that             = this;

	if (!$sel.length) {
		return;
	}
	
	// Found a row to show - not a new question
	$navBtns.prop('disabled', true);
	function doneMove () {
		$navBtns.prop('disabled', false);
		that.adjustMobilePageControls();
	}
	if ($.support.transition) {
		var suffix = goPrev ? 'Prev' : 'Next'
			, outClass = 'sg-page-rotateSlideOut' + suffix
			, inClass = 'sg-page-rotateSlideIn' + suffix;
		$vis
		.addClass(outClass)
		.one($.support.transition.end, function() {
			$vis.removeClass(currentClass + ' ' + outClass).addClass(otherClass);
		}).emulateTransitionEnd(1000);
		
		$sel
		.addClass(inClass + ' ' + currentClass)
		.removeClass(otherClass)
		.one($.support.transition.end, function() {
			$sel.removeClass(inClass);
			doneMove();
		}).emulateTransitionEnd(1000);
	}
	else {
		$row.removeClass(currentClass).addClass(otherClass);
		$sel.removeClass(otherClass).addClass(currentClass);
		doneMove();
	}
	return true;
};

SGSurvey.prototype.goMobilePage = function ($next) {
	var currentClass = 'sg-page-current'
		, $current = $SG('.' + currentClass)
		, goPrev = $next == -1
		, $navBtns = $SG('.sg-mobile-next, .sg-mobile-back');
	
	// Check that prevent default was not called for paging
	if (!this.eventCanPage(goPrev ? -1 : 1)) {
		return;
	}
	
	// If it is a table, go to the next/prev row
	if ($current.hasClass('sg-type-table') 
		&& !$current.hasClass('sg-type-rank-table')
		&& !($next instanceof $SG) 
		&& this.goMobileTableRow($current, goPrev)
	) {
		return;
	}
	
	// If it is max diff, go to the next/prev set
	if ($current.hasClass('sg-type-maxdiff') 
		&& !($next instanceof $SG) 
		&& !goPrev
	) {
		if ($current.find('.sg-maxdiff-set:visible').nextAll('.sg-maxdiff-set').length) {
			return;
		}
	}
	
	
	if($next instanceof $SG && $next.length){
		$next = $next
	}
	else{
		// if server side validation error
		if($('.sg-error-message.sg-error-display:not(.mobile-error-msg)').length){
			$next = $current[goPrev ? 'prevAll' : 'nextAll']('.sg-question.sg-errors:visible,.sg-question.sg-warning:visible').first()
		}
		else{
			$next = $current[goPrev ? 'prevAll' : 'nextAll']('.sg-question:visible:first')
		}
	}
	// Current can't be the same as next
	if (!$next.length || $next.hasClass(currentClass)) {
		return;
	}
	
	// Pre-adjust the table rows before showing it
	this.adjustMobileTableRows($next, goPrev ? 'last' : 'first');
	
	var isAfter = $current.nextAll('.sg-question').filter($next[0]).length > 0
		, suffix = isAfter ? 'Next' : 'Prev'
		, outClass = 'sg-page-rotateSlideOut' + suffix
		, inClass = 'sg-page-rotateSlideIn' + suffix
		, that = this;
	
	$navBtns.prop('disabled', true);
	function doneMove () {
		$navBtns.prop('disabled', false);
		that.adjustMobilePageControls();
		 //scroll to top
		        $SG('body, html, .sg-outer-wrapper, .sg-wrapper').animate({
		            scrollTop: 0
		        }, 100);
	}

	this.mobileMoveOutPage($current, isAfter);
	this.mobileMoveInPage($next, isAfter, doneMove);
	
	//need to flag seen pages
	if (!$current.hasClass('sg-page-shown')) {
		$current.addClass('sg-page-shown');
	}
	if (!goPrev && !$next.hasClass('sg-page-shown') && !$SG('body').hasClass('sg-mobile-offline')) {
		this.updateProgressBar();
	}
};

SGSurvey.prototype.mobileMoveInPage = function ($next, isAfter, doneMove) {
	if (!$next.length) return;
	doneMove = doneMove || $.noop;
	var inClass = 'sg-page-rotateSlideIn' + (isAfter ? 'Next' : 'Prev')
		, currentClass = 'sg-page-current'
		, that = this;
	if ($.support.transition) {
		$next
		.addClass(inClass + ' ' + currentClass)
		.one($.support.transition.end, function() {
			$next
			.removeClass(inClass)
			.addClass('sg-page-shown')
			/*.css({
				position : ''
			});*/
			
			that.adjustMobilePageControls();
			doneMove();
		}).emulateTransitionEnd(1000);
	}
	else {
		$next.addClass(currentClass);
		this.adjustMobilePageControls();
		doneMove();
	}
};

SGSurvey.prototype.mobileMoveOutPage = function ($current, isAfter, doneMove) {
	if (!$current.length) return;
	doneMove = doneMove || $.noop;
	var outClass = 'sg-page-rotateSlideOut' + (isAfter ? 'Next' : 'Prev')
		, currentClass = 'sg-page-current'
		, that = this;
	if ($.support.transition) {
		$current
		.addClass(outClass)
		.one($.support.transition.end, function() {
			$current
			.removeClass(currentClass + ' ' + outClass)
			//.css('position', '');
			
			that.adjustMobilePageControls();
			doneMove();
		}).emulateTransitionEnd(1000);
	}
	else {
		$current.removeClass(currentClass);
		this.adjustMobilePageControls();
		doneMove();
	}
};


/** 
* 	Timer
* 	This function runs times events that check the stust of rules placed in the doLoop
**/
SGSurvey.prototype.Timer = function ()
{
	if(this.isEditor == true)return;
	/* Setup dynamic logic and handlers on the page */
	for(var q = 0; q < this.doLoop.length;q++)
	{
		this.doLoop[q].rule();
	}
}
/**
* add a timer rule to the current survey object in js
*
**/
SGSurvey.prototype.add_rule = function (r)
{
	this.doLoop.push(r);
}
/**ported from global functions in old survey render 
* adds an error for an element id (question)
*
*/
SGSurvey.prototype.add_error = function (elementid,id,message) //old function: sgjsAddError
{ 

    this.remove_error(elementid,id);
    if( typeof this.page_errors[elementid]  == 'undefined') this.page_errors[elementid] = [];
    this.page_errors[elementid].push([id,message]);
    if( typeof this.total_errors == 'undefined') this.total_errors = 0;
    this.total_errors++;      
}
/**ported from global functions in old survey render 
* removes an error for an element id (question)
*
*/
SGSurvey.prototype.remove_error = function (elementid,id)
{
    if( typeof this.page_errors[elementid]  == 'undefined') this.page_errors[elementid] = [];
    var newarray = Array();
    
    for(var i = 0; i < this.page_errors[elementid].length;i++)
    {
        if(this.page_errors[elementid][i][0] != id) newarray.push(this.page_errors[elementid][i]);
        else {this.total_errors--;}
    } 
	this.page_errors[elementid] = newarray;                
}
/**
* ported.  Counts the number of errors for an element id (question)
**/
SGSurvey.prototype.error_count = function (elementid) //sgjsErrorCount
{
	if( typeof this.page_errors[elementid]  == 'undefined') this.page_errors[elementid] = [];
	return this.page_errors[elementid].length;
}
/**ported from global functions in old survey render 
* updates the element error display
*
*/
// !update
SGSurvey.prototype.update_element_errors = function (element,msg) //sgjsUpdateElementErrors
{
	var current_id = element.id;
	var num = this.error_count(current_id);
	var msg = $SG('#'+$SG(element).attr('id')+'-message');
	if(msg.attr('id') !== $SG(element).attr('id')+'-message')
	{
		$SG(element).parent().append('<div id="'+$SG(element).attr('id')+'-message" class="sg-message" style="display:none;"></div>');
		msg = $SG('#'+$SG(element).attr('id')+'-message');
	}
	if(num > 0)
	{
	    arr = this.page_errors[current_id]?this.page_errors[current_id]:[];
	    msg.html('');
	    for(var i = 0; i < arr.length;i++)
	    {
	        if(arr[i][1] != "")
	        {
	        	$SG("<div></div>").text(arr[i][1]).appendTo(msg);
	        }
	    }         
	    if(msg != "" && typeof msg != "undefined")
	    {
	        msg.parent().removeClass('sg-validation-pass').addClass('sg-validation-fail');
	        msg.show();
	    }              
	}
	else
	{
	    if(msg != "" && typeof msg != "undefined")
	    {
	       	msg.html('').parent().removeClass('sg-validation-fail').addClass('sg-validation-pass');
	       	msg.hide();
	    }
	}
}

/** 
 *
 * Forward Only Survey **/
SGSurvey.prototype.checkForwardOnly = function ()
{
  return;

	if(this.surveyObject.options.forward_only)
	{
		var cookievalue = SGCookie("path" + this.surveyObject.session);
		if(cookievalue == null)cookievalue = "S";
				
		var page_list = cookievalue.split(",");
		var lastpage = page_list.pop();
		
		if(lastpage != this.surveyObject.currentpage && $SG.inArray(this.surveyObject.currentpage,page_list) >= 0)
		{
			$SG("#sgbody-" + this.surveyObject.id ).hide();
			document.location = $SG("#sg_FormFor"+this.surveyObject.id).get(0).action + ($SG("#sg_FormFor"+this.surveyObject.id).get(0).action.indexOf("?") > 0 ? "&" : "?") + "id=" +  this.surveyObject.id + "&sg_navigate=current_page";
			return;
		}

		if(lastpage != this.surveyObject.currentpage)page_list.push(lastpage);  
		page_list.push(this.surveyObject.currentpage);
		SGCookie("path" + this.surveyObject.session,page_list.join(","));		
	}
}

/**
 * Get the value of a cookie with the given name.
 *
 * @example SGCookie('the_cookie');
 * @desc Get the value of a cookie.
 *
 * @param String name The name of the cookie.
 * @return The value of the cookie.
 * @type String
 */
SGCookie = function(name, value, options) {

    if (typeof value != 'undefined') { // name and value given, set cookie
     
        options = options || {};
        if (value === null) {
            value = '';
            options.expires = -1;
        }
        var expires = '';
        if (options.expires && (typeof options.expires == 'number' || options.expires.toUTCString)) {
            var date;
            if (typeof options.expires == 'number') {
                date = new Date();
                date.setTime(date.getTime() + (options.expires * 24 * 60 * 60 * 1000));
            } else {
                date = options.expires;
            }
            expires = '; expires=' + date.toUTCString(); // use expires attribute, max-age is not supported by IE
        } 
        // CAUTION: Needed to parenthesize options.path and options.domain
        // in the following expressions, otherwise they evaluate to undefined
        // in the packed version for some reason...
        var path = options.path ? '; path=' + (options.path) : '';
        var domain = options.domain ? '; domain=' + (options.domain) : '';
        var secure = options.secure ? '; secure' : '';
        document.cookie = [name, '=', encodeURIComponent(value), expires, path, domain, secure].join('');
   
    } else { // only name given, get cookie
        var cookieValue = null;
        if (document.cookie && document.cookie != '') {
            var cookies = document.cookie.split(';');
            for (var i = 0; i < cookies.length; i++) {
                var cookie = $SG.trim(cookies[i]);
                // Does this cookie string begin with the name we want?
                if (cookie.substring(0, name.length + 1) == (name + '=')) {
                    cookieValue = decodeURIComponent(cookie.substring(name.length + 1));
                    break;
                }
            }
        }
        return cookieValue;
    }
};
 
/**
 * This function is responsible for returning the total of the continuous sum.
 * @author dehru -- modifications by Matt Null
 * @properties -- the question's properties
 * @return total
 * 
 */
SGSurvey.prototype.getContinuousSumTotal = function(el,properties) {
	properties  = properties || null;
	var onlyWholeNumbers = properties.only_whole_num || null;

	stotal=0;
	$SG(el).parents('.sg-type-continuous-sum').find('input.sg-input-text').each(function(i, element, onlyWholeNumbers){
					var basevalue = $SG.trim(this.value.replace(/[^\-\.\d]/g,''));
					if(onlyWholeNumbers)
						stotal += isNaN(parseInt(basevalue)) ? 0 : parseInt(basevalue);
					else
						stotal += isNaN(parseFloat(basevalue)) ? 0 : parseFloat(basevalue);
				});
	
	//Matt Null added this so avoid very long decimal places          
	if(!onlyWholeNumbers)
		stotal = stotal.toFixed(2);
	
	//Matt Null append appropriate symbol to the total
	if(properties.force_currency == true){	
		stotal = '' + currency_glyph_global + stotal;
	}
	else if(properties.force_percent == true){
		stotal = stotal+'%';
	}

	return stotal;
}

/**
 * This function is responsible for the display changes based on the continuous sum.
 * @author dehru
 * @param domElement element
 * @param float stotal
 * @param questionObject question
 */
SGSurvey.prototype.displayContinuousSum = function(element, stotal, question) {
	element.html(stotal);
	var showError = false;
	if(stotal>question.properties.max_total && question.properties.max_total)
	{
		showError = true;
	}
	
    if(question.properties.must_be_max && stotal!=question.properties.max_total)
    {
    	showError = true;
    }
    
    if (showError) 
    	$SG(element).parent().addClass("sg_Counter_Error");
    else
    	$SG(element).parent().removeClass("sg_Counter_Error");
}


/**
 * Used as callback method to onBlur, also supporting method for other number formatters
 */
SGSurvey.prototype.formatNumber = function(target, properties, format) {
	var element = $SG(target);
	// sanitize input so we can process it
	if(element.val().match(/,\d{2}$/)) {
		var num = element.val().replace(/[^\-\,\d]/g,'').replace(/\,/, ':').replace(/\,/g, '').replace(/\:/, '.'); // Euro validation. convert comma to decimal for now.
	} else {
		var num = element.val().replace(/[^\-\.\d]/g,'').replace(/\./, ':').replace(/\./g, '').replace(/\:/, '.');
	}
	// what to do with waht we got
	if($SG.trim(element.val()) == '') {
		// don't let only spaces get through
		num = '';
		//$SG(target).removeClass('sg-validation-error');
	} else if($SG.trim(num) == '' || isNaN(Number(num))) {
		// flag as error
		$SG(target).addClass('sg-validation-error');
		num = element.val();
	} else {
		// go go go
		var prepend = '';
		var append = '';
		var is_negative = false;
		
		// do we have a negative number
		if(num != Math.abs(num) && properties.only_positive_num !== true) {
			is_negative = true;
		}
		
		// strip off modifier (we'll add it later if needed)
		num = num.replace(/\-/g, '');
		
		// round for "whole numbers only" validation
		if(properties.only_whole_num) {
			num = Math.round(num);
		}
		
		// accessorize the values
		if(format == 'percent') {
			append = '%';
		}
		
		if(format == 'currency') {
			prepend = currency_glyph_global;
			if(!properties.only_whole_num) {
				num = Math.floor(num*100+0.50000000001);
				cents = num%100;
				num = Math.floor(num/100).toString();
				if(cents<10) {cents = "0" + cents;}
				
				for (var i = 0; i < Math.floor((num.length-(1+i))/3); i++) {
					num = num.substring(0,num.length-(4*i+3))+',' + num.substring(num.length-(4*i+3));
				}
				num = num + '.' + cents;
			}
		}
		$SG(target).removeClass('sg-validation-error');
		num = (is_negative ? '-' : '') + prepend + num + append;
		//element.val(num)
	}
	
	return num;
}


/**
 * Used as callback method to onBlur
 */
SGSurvey.prototype.formatPercent = function(target, properties) {
	var num = this.formatNumber(target, properties, 'percent');
	return num;
}


/**
 * Used as callback method to onBlur
 */
SGSurvey.prototype.formatCurrency = function(target, properties) {
	var num = this.formatNumber(target, properties, 'currency');
	return num;
}

/** A Better way to hook validation functions to targets **/
SGSurvey.prototype.InitValidationHooks = function () {
	$SG(".sg-validation-date").each(function (name,item)
			{
				var date_format = "%m/%d/%Y"
				if($(this).hasClass('sg-validation-date-eu'))
					date_format = "%d/%m/%Y"
				var calandericonid = (new String (item.id)).replace("-element","Trigger");
				if(!$('body').hasClass('sg-mobile')) {
					var mindate = $(this).data('mindate')
					var maxdate = $(this).data('maxdate')
					
					cal = Calendar.setup({
						inputField : item.id,
						min: mindate,
						max: maxdate
					});
					
					cal.manageFields(calandericonid, item.id, date_format);
					//trigger a change event on the textbox for page logic and custom scripting 
					cal.addEventListener('onSelect',
						function(cal) {
							cal.hide(); 
							$('#'+item.id).trigger( "change" )
						}
					)
				}
			});
	
	/**
	 * autocomplete uses jquery.autocomplete.js library
	 * @author dehru
	 */
	$SG(".sg-autocomplete").each(function (name,item)
	{
		/** find question **/
		var question = null;
		var surveyId = (new String(item.id)).split("-")[1];
		var surveyobj = SGAPI.surveyData[surveyId];
		var parent_sku = (new String(item.id)).split("-")[3];
		
		for(var qi in surveyobj.questions)
		{
			if(surveyobj.questions[qi].id==parent_sku)
			{
				var base = "#" + item.id;
				question = surveyobj.questions[qi];
			}
		}
		if(question == null)return true;
	
		if (question.properties.autocomplete.length > 0 && question.properties.autocomplete.substring(0,1) == '@')
		{
			//$SG(base).autocomplete("https://appv3.sgizmo.com/http/filesearch?file=" + question.properties.autocomplete.substring(1));
			$SG(base).autocomplete("/s3/ajax_proxy.php?call=filesearch&file=" + question.properties.autocomplete.substring(1));
			
		} else if (question.properties.autocomplete.length > 0 && question.properties.autocomplete.indexOf("\n") > 0) 
		{
			$SG(base).autocomplete(question.properties.autocomplete.split("\n"));
		}
		
		if(question.properties.force_autocomplete && question.properties.autocomplete.length > 0){
			$SG(this).blur(function(){
				var inlist = false;
				var list = '';
				if(question.properties.autocomplete.substring(0,1) == '@')
				{
					var this_value = this;
					$SG.ajax({
						url : "/s3/ajax_proxy.php?call=filesearch&file=" + question.properties.autocomplete.substring(1) + "&q=" + $SG(this_value).val(),
						success : function(res){
							list = res.split("\n");
							for(var i in list)
							{
								if($SG(this_value).val() == $.trim(list[i]))
								{
									inlist = true;
									break;
								}
							}
							if(!inlist){
								$SG(this_value).val('');
							}
						}});
				}
				else
				{
					list = question.properties.autocomplete.split("\n");
					for(var i in list)
					{
						if($.trim($SG(this).val()) == $.trim(list[i]))
						{
							inlist = true;
							break;
						}
					}
					if(!inlist){
						$SG(this).val('');
					}
				}
			});
		}
	}); 
}

/**
* Uploads a file to the webserver via background iframe
*
*
**/
SGSurvey.prototype.UploadFile = function(name,cid,key,maxfiles,maxfilesize,extentions,type,size,nofile)
{
	var url = ''; 
	SO = this;
	maxfiles = maxfiles || null;

	if($SG('#'+name+'-element').val() != '')
	{
	
		//Start spinner
		$SG('#'+name+'-box .sg-file-feedback').fadeIn(function(){$SG(this).removeClass('sg-hide')}).addClass('sg-file-loader').html(SO.messages.uploading_file);
		
		if(this.surveyObject.ispreview == 'true')
		{
			url = '/projects/previewupload?input_name='+name+'&cid='+cid+'&key='+key+'&uuid='+this.surveyObject.session;
		}
		else
		{
			url = '/s3/file500/?input_name='+name+'&cid='+cid+'&key='+key+'&uuid='+this.surveyObject.session+'&maxfilesize='+maxfilesize+'&extentions='+extentions+'&error_filetype='+type+'&error_size='+size+'&error_nofile='+nofile;
		}
		
		var can_add = false;
		$SG('.'+name).each(function()
		{
			if($SG(this).val()=='')
			{
				can_add = true;
				return false;
			}
		});
		
		if(can_add == true)
		{
			//context
			var c = this;
			$SG.ajaxFileUpload(
			{
				url:url, 
				secureuri:false,
				fileElementId:name+'-element',
				dataType: 'json',
				success: function (data, status)
				{ 
					
					//stop spinner gif
					$SG('#'+name+'-box .sg-file-feedback').fadeOut().removeClass('sg-file-loader');
					
					if(data.error != undefined && data.error != '')
					{
						c.UploadShowAlert(name,data.error);
					}
					else if(data.success.file_name == '')
					{
						c.UploadShowAlert(name,SO.messages.upload_error);
					}
					else
					{
						if(maxfiles != null){ 
							//if(SO.count_uploaded_files >= parseInt(maxfiles) || $SG('#sg-file-desc-'+name+' ul').children('li').length >= parseInt(maxfiles)){
							//removed comparing to SO.count_uploaded_files because it was counting all files on the page instead of files in a specific question
							if($SG('#sg-file-desc-'+name+' ul').children('li').length >= parseInt(maxfiles)){
								$SG('#'+name+'-element').parent('div').css("display","none");
							} 
						}
						
						SO.count_uploaded_files++;
						//Add file name to hidden input
						$SG('.'+name).each(function()
						{
							if($SG(this).val()=='')
							{
								$SG(this).val(data.success.file_name.replace(/\'|"/g,""));
								return false;//Acts like a break
							}
						});
						//Add li for new file
						if($SG('#sg-file-desc-'+name+' ul').children('li').length < 2)
							$SG('#sg-file-desc-'+name).removeClass('sg-hide');
						
						var li_html = $SG('#sg-file-desc-'+name+' .sg-prototype-li').clone().removeClass('sg-prototype-li');
						$SG(li_html).html($SG(li_html).html().replace('{{filename}}',data.success.name));
						$SG(li_html).find('a').attr('rel',data.success.file_name);
						$SG('#sg-file-desc-'+name+' ul').append(li_html);
						$SG('#'+name+'-element').val('');

						var file_name = data.success.file_name;
						//Matt Null -- attach delete button event and do the delete
						$SG(li_html).find('.sg-file-delete').click(function(){
							c.deleteUploadedFile(data,name,this);
						});
					}
				},
				error: function (data, status, e)//General Upload Error
				{
					var response = $SG.parseJSON(data.responseText);
					
					if(status == 'error' && response.error == 'filetype'){
											c.UploadShowAlert(name,SGAPI.util.translate(SO.messages.invalid_file_type));
					}
					else if(status == 'error' && response.error == 'filesize'){
						c.UploadShowAlert(name,SGAPI.util.translate(SO.messages.invalid_file_size));
					}
					else{
						c.UploadShowAlert(name,SGAPI.util.translate(SO.messages.upload_error));
					}
				}
			});
		}
		else//Max files reached
		{
			$SG('#'+name+'-element').val('');
			this.UploadShowAlert(name,SO.messages.no_more_files);
		}
	}
	else//No file specified
	{
		this.UploadShowAlert(name,SO.messages.no_file_specified);
	}
}

SGSurvey.prototype.SetupFileUpload = function (name, customerId, key, maxFiles, maxFileSize, extensions, type, size, noFile)
{
	var $input    = function () { return $SG('#' + name + '-element'); }
	,	$desc     = $SG('#sg-file-desc-' + name)
	,   $button   = $input().parent().siblings('.sg-upload-btn')
	,	$filename = $input().parent().siblings('.sg-browse-text')
	,   $feedback = $SG('#' + name + '-box .sg-file-feedback')
	,	that      = this
	,   fileData;

	var url = '/s3/file500/?' + $.param({
		'input_name' : name
		, 'cid' : customerId
		, 'key' : key
		, 'uuid' : this.surveyObject.session
		, 'maxfilesize' : maxFileSize
		, 'extentions' : extensions
		, 'error_filetype' : type
		, 'error_size' : size
		, 'error_nofile' : noFile
	});

	if (this.surveyObject.ispreview == 'true') {
		url = '/projects/previewupload?' + $.param({
			'input_name' : name
			, 'cid' : customerId
			, 'key' : key
			, 'uuid' : this.surveyObject.session
		});
	}

	$input().fileupload({
		forceIframeTransport: true,
        url : url,
        autoUpload : false,
        maxFileSize : maxFileSize
        
        , success : function (data, status) {
			$feedback
			.fadeOut()
			.removeClass('sg-file-loader');

			if (data.error != undefined && data.error != '') {
			    that.UploadShowAlert(name, data.error);
			}
			else if (data.success.file_name == '') {
			    that.UploadShowAlert(name, that.messages.upload_error);
			}
			else {
			    if (maxFiles != null && $SG('ul', $desc).children('li').length >= parseInt(maxFiles)) {
		            $input().closest('div').hide();
			    }

			    that.count_uploaded_files++;

				// Add file name to hidden input
				$SG('.' + name).each(function() {
					if ($SG(this).val() == '') {
						$SG(this).val(data.success.file_name.replace(/\'|"/g, ""));
						return false;
					}
				});

			    // Add li for new file
			    if ($SG('ul', $desc).children('li').length < 2) {
			        $desc.removeClass('sg-hide');
				}

			    var $li = $SG('.sg-prototype-li', $desc).clone().removeClass('sg-prototype-li');
			    $SG($li).html($SG($li).html().replace('{{filename}}', data.success.name));
			    $SG($li).find('a').attr('rel', data.success.file_name);
			    $SG('ul', $desc).append($li);
			    $input().val('');

			    $SG($li).find('.sg-file-delete').click(function () {
					that.deleteUploadedFile(data, name, this);
			    });
			}
        }
        
        , error: function () {
	        that.UploadShowAlert(name, that.messages.upload_error);
        }
    })
    .prop('disabled', !$.support.fileInput)
    .parent()
    .addClass($.support.fileInput ? undefined : 'disabled');

    $input().bind('fileuploadadd', function (e, data) {
    	$filename.text(data.files[0].name);
        fileData = data;
    });

    $button.click(function() {
    	if (fileData) {
	    	// Start spinner
			$feedback.fadeIn(function() {
				$SG(this).removeClass('sg-hide')
			})
			.addClass('sg-file-loader')
			.html(that.messages.uploading_file);

			fileData.submit();
    	}
    	else {
	    	that.UploadShowAlert(name, that.messages.no_file_specified);
    	}
		fileData = null;
	});
};

//Matt Null -- deletes and uploaded file
SGSurvey.prototype.deleteUploadedFile = function(data,name,el){
	data = data || {};
	var filename = data.success.file_name,
		c = this;
	
	if(el.target){
		el = el.target;
	}
	else if(el.srcElement){
		el = el. srcElement;
	}
	else{
		el = el;
	}
	
	$SG.ajax({
		url: '/s3/delete_file.php',
		data : {file : filename,cid : c.surveyObject.customer_id, sid : c.surveyObject.id},
		type : 'POST',
		beforeSend : function(){
			c.UploadShowAlert(name, 'Deleting...');
		},
		success : function(){
			$SG(el).parent('li').remove(); 
			
			if($SG('#sg-file-desc-'+name).find('li').length == 1)
				$SG('#sg-file-desc-'+name).addClass('sg-hide');
				
			$SG('.'+name).each(function()
			{
				if($SG(this).val() == filename)
				{
					$SG(this).val('');
					return false;
				}
			});
		}
	});
	var $par = $SG('#' + name + '-element').closest('div');
	$par.show();
	$SG('.sg-browse-text', $par).text('');
	this.count_uploaded_files--;
}

SGSurvey.prototype.UploadShowAlert = function(name,msg)
{
	$SG('#'+name+'-box .sg-file-feedback').removeClass('sg-file-loader').fadeIn(function(){$SG(this).removeClass('sg-hide')}).addClass('sg-file-alert').html(msg);
	setTimeout(function(){$SG('#'+name+'-box .sg-file-feedback').fadeOut(2000).removeClass('sg-file-alert');},3000);
}

/** 
* Function:  InitializeQuestion 
* sets up the question with the appropriate click handlers etc.  This does a lot of setting up, so it's probably
* going to be one that gets checked/used a lot 
* @param Object question
**/
SGSurvey.prototype.InitializeQuestion = function (question, admin_view) { 
	var self = this;
	//skip over questions that do not exist.
	if(typeof question == 'undefined') {
		return false;
	}
	
	var base = "#sgE-" + this.surveyObject.id + "-" +  this.page_id +'-'+ question.id, ansFirst;
	
	/**
	 * Calculates the elapsed time (ms) before answering 
	 * a question
	 * @return void
	 */
	function answeredQuest () {
		if (ansFirst) return;
		ansFirst = !0;
		var n = new Date(), $t = $SG(base + '-time');
		if ($t.val()) return;
		$t.val((n.getTime() - self.lastChangeTime.getTime()) / 1000);
		self.lastChangeTime = n;
	}
	$SG(base + '-box').on('change', ':input', answeredQuest);

	// to fake a one-input vs. options... one input calls its id -element, many options have -sku
	// pFred- what??
	if (typeof question.options == 'undefined' && question.type!='MENU') {
		question.options = {0:{id:'element'}}; 
	}
	
	if(question.properties.complicated_logic_rule != undefined && question.properties.complicated_logic_rule_triggers != undefined){
		SGAPI.survey.initLogic(question, question.properties.complicated_logic_rule, question.properties.complicated_logic_rule_triggers);
	}


			var newArr = [],
			len = arguments.length, 
			arg;

			for (var i=0; i<len; i++) {			

				arg = arguments[i];

				if (arg instanceof Array) {

					for (var k=0, l=arg.length; k<l; k++) {

						newArr = newArr.concat(this.union(arg[k]));
					}
				}
				else {
					newArr.push(arg)
				}
			}
			return this.unique(newArr);

			var isWindow = (el == window) ? true : false;

			var $el = $(el);
			
			if ($el.length == 0) {
				return;
			}
			
			var offset = isWindow ? {top : 0, left : 0} : $el.offset();
			var width =  isWindow ? $el.width() :$el.outerWidth(true);
			var height = isWindow ? $el.height() : $el.outerHeight(true);

			var $indicator = $('<div class="loading-indicator"></div>')
				.append(
					$('<div class="img" style="position:absolute;left:'+(width/2-8)+'px;top:'+(height/2-11/2)+'px;"></div>')
				);
	
			if (!isWindow && ($el.css('position') == 'relative' || $el.css('position') == 'absolute')) {
				$indicator.css({
					top: 0,
					left: 0,
					right: 0,
					bottom: 0
				})
				.appendTo($el);
			} else {
				$indicator.css({
					top: offset.top,
					left: offset.left,
					width: width,
					height: height
				})
				.appendTo('body');
			}

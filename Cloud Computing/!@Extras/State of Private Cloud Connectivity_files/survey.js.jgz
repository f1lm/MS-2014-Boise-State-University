//Matt Null - force no repeats
SGSurvey.prototype.force_no_repeats = function(el) {
	
	el.each(function(){
		var parentEl = $(this);
		
		//only looking for selects now... can easily make the selector "select,input" etc
		parentEl.find('select').each(function() {
			
			$(this).change(function() {				
				var val = $(this).prop('selectedIndex');
				
				parentEl.find('select').not(this).each(function() {
					
					if (val == $(this).prop('selectedIndex')) {
						$(this).prop('selectedIndex', 0);
					}
				});
			});
		});
	});
}

/**
 * @author Matt Null
 * Handles all of the kiosk JS
 */
SGSurvey.prototype.runKioskMode = function(){
	var kioskIntro = $('#kiosk-intro');
	var kioskCloseButton = $('#kiosk-close-button');

	//attach events to top kiosk bar	
	$('#kiosk-start-new-survey').click(function(){
		if(window.location.href.slice(window.location.href.indexOf('?')).length > 1){
			window.location.href = window.location.href;
		}
		else{
			window.location.href = window.location.href + '?__output=kiosk';
		}
	});
	
	//if this is the first page
	if(kioskIntro.size() > 0){
		kioskIntro.show();
		if($SG('body').hasClass('sg-mobile-optimized')){
			$('.sg-question-set').find('.sg-question').first().hide();
		}
		else{
			$('.sg-question-set').find('.sg-question').hide();
		}
		$('.sg-footer').hide();
		
		$('#kiosk-start-button').click(function(){
			kioskIntro.hide();
			$('.sg-question-set').find('.sg-question:not(.sg-hide)').slideDown(500);
			$('.sg-footer').fadeIn(500);
		});
	}
	else if(kioskCloseButton.size() > 0){ //its the last page
		this.KioskFinalPageCounter();
		kioskCloseButton.click(function(){
			window.location.href = window.location.href.replace('?__output=kiosk','') + '?__output=kiosk';
		});
	}
	
	//run the idle timeout script for kiosk
	if(this.surveyObject.currentpage != 1){
		var c = this;
		function startIdleTimeout(){
			window.kioskIdleTimeout = setTimeout(function(){
				if(window.location.href.slice(window.location.href.indexOf('?')).length > 1){
					window.location.href = window.location.href;
				}
				else{
					window.location.href = window.location.href + '?__output=kiosk';
				}
			}, c.surveyObject.options.is_kiosk.idle_timeout * 60000);
		}
		
		startIdleTimeout();
		
		//if the user interacts with the window, restart the idletimeout
		$(window).click(function(){
			window.clearTimeout(kioskIdleTimeout);
			startIdleTimeout();
		});
	}
};

/**
 * @author Matt Null
 * Handles Kiosk last page timer
 */
SGSurvey.prototype.KioskFinalPageCounter = function(){
	var countdownContainer = $('#kiosk-countdown');
	var finalPageCounter = parseInt(countdownContainer.html());
	setInterval(function(){
		var count = parseInt(countdownContainer.html());
		
		if(count == 0){
			return;
		}
		
		countdownContainer.html(count - 1);
	}, 1000);
	
	setTimeout(function(){
		window.location.href = window.location.href.replace('?__output=kiosk','')+ '?__output=kiosk';
	},finalPageCounter * 1000);
};

/** 
*	This function gets the target of a particular event (most event handlers need this to work properly)
**/
function sgGetEventTarget(event)
{
	var targetElement = null;
	if(typeof event.target != "undefined")
	{
		targetElement = event.target;
	} 
	else 
	{
		targetElement = event.srcElement;
	};
	while(targetElement.nodeType == 3 && targetElement.parentNode != null)
	{
		targetElement = targetElement.parentNode;
	};
	return targetElement;
}

	
var Survey = function() {}

Survey.prototype = {	

	/** 
	 * This function must be called after the class is instantiated.
	 * 
	 * @param {obj} oArgs
	 */
	init: function(oArgs) {
		this.sgSurvey = oArgs.sgSurvey;
		this.surveyObject = oArgs.surveyObject;
		this.pageId = oArgs.pageId;
		this.optionDepsMap = {};
		this.dNumbering = this.sgSurvey.dNumbering;
		this.decremented = [];
	},
	
	/**
	 * This function updates the meta element of the 'target'.
	 * 
	 * @param {$, string} target
	 * @param {string} token
	 * @param {string} val
	 * 
	 */
	setMeta: function (target, token, val) {

		if (target instanceof $) {
			target = '#' + target.attr('id');
		}

		$(target.replace(/-box/g,'-meta')).each(function() {
			var exp = new RegExp(token+'=(.+?)&|$')
			var m = exp.exec(this.value);
			
			// was a match, it exists in there
			if (m.length>1) {
				this.value = this.value.replace(token + '=' + m[1], token + '=' + val);
			}
			else { //no match, add it in			
				this.value += '&' + token + '=' + val;
			}
		});
	},
	
	/**
	 * This function triggers the "select all" for "all of the above" options. 
	 */
	setAllOfAbove: function() {
		
		var el = $( this );
		var isOn = el.is( ':checked' );
		var questEl = el.parents( '.sg-question:first' );
		var options = questEl.find( 'input:checkbox' );

		options.each( function() {
			
			var option = $( this )
			
			if ( option.data( 'special' ) ) {
				return;
			}
			
			option.prop( 'checked', isOn );
			
			if ( isOn ) {			
				option.bind( 'click.resetAllAbove', function() {
					el.prop( 'checked', false );
					options.unbind( '.resetAllAbove' );
				});
			}
			else {		
				option.unbind( '.resetAllAbove' );
			}
		});

		SGAPI.survey.checkQuestionDependents();
	},
	
	//decrement question numbers after a show/hide trigger
	decrementNumbers : function(question) {
		if(question.find('.sg-question-number').length > 0){
			if(question.find('.sg-question-number').html().length > 0){
				question.nextAll().find('.sg-question-number').each(function(){
					newNum = ($(this).html().replace('.','') * 1) - 1;
					$(this).html(newNum + '.');
				});
			}
		}
	},
	
	//increment question numbers after a show/hide trigger
	incrementNumbers : function(question) {
		if(question.find('.sg-question-number').length > 0){
			//no need to do this if the shown question is last on the page
			if(question.attr('id') == $('.sg-question').not('.sg-type-instruction').last().attr('id')){
				return;
			}

			if(question.find('.sg-question-number').html().length > 0){
				var lastNumElem = $('.sg-question .sg-question-number:last');
				if (!lastNumElem.length) {
					return;
				}
				
				var lastQuestion = lastNumElem.closest('.sg-question');
				
				if (question.closest('.sg-type-group').length && lastQuestion.closest('.sg-type-group').length) {
					return;
				}
				
				var lastNum = lastNumElem.html().replace('.','') * 1 + 1;
				lastNumElem.html(lastNum + '.');
				
				lastQuestion.prevUntil(question).find('.sg-question-number').each(function(){
					var newNum = ($(this).html().replace('.','') * 1) + 1;
					$(this).html(newNum + '.');
				});
			}
		}
	},
	
	/**
	 * This function hides all the dependent questions passed to it.
	 * It also clears the data for the hidden questions.
	 * 
	 * @param {obj} deps
	 * 
	 */	
	hideDependents: function(deps, click) {
		this.placeHolderQuestVals = this.placeHolderQuestVals || {};
		var that = this, 
			updatePlaceHolders = function (box) { // Remember the values of the questions so showDependents can repopulate if it needs to
				box = $(box);
				var id = box[0].id;
				if (box.find(':checkbox').length) {
					that.placeHolderQuestVals[id] = [];
					box.find(':checkbox:checked').each(function(){ that.placeHolderQuestVals[id].push($(this).val()) })
					if (!that.placeHolderQuestVals[id].length) delete that.placeHolderQuestVals[id];
				} else if (box.find(':radio').length) {
					that.placeHolderQuestVals[id] = box.find(':radio:checked').val();
				} else {
					if (box.find('.sg-input').is('select') && box.find('.sg-input').val() == 'NoAnswer') return;
					that.placeHolderQuestVals[id] = box.find('.sg-input').val();
					if (that.placeHolderQuestVals[id] == '') delete that.placeHolderQuestVals[id];
				}
			};
		
		$.each(deps, function(id, dep){
			dep.hide();

			that.setMeta(dep, 'hidden', 'true');				
			if (click) {
				// clean question data except for textboxes (yeah whatever)
				that.clearQuestionDataOld(dep);
			} else if (deps.constructor != Array 
				&& !!dep.find('.sg-input').length 
				&& that.placeHolderQuestVals[id] == undefined
			) { // Remember default (placeholder) values
				if (dep.hasClass('sg-type-group')) {
					dep.find('.sg-question')
						.each(function(){
							updatePlaceHolders(this);
						})
				} else {
					updatePlaceHolders(dep);
				}
			}
			
			// if the dep is a group, hide all questions inside
			if (dep.hasClass('sg-type-group')) {
				var subQuests = [];
				
				dep.find('.sg-question').each(function() {
					subQuests.push($(this));
				});
				
				that.hideDependents(subQuests, click);
			}
		})
	},
	
	/**
	 * This function shows all the dependent questions passed to it.
	 * 
	 * @param {$, obj} deps
	 * 
	 */		
	showDependents: function(deps) {
		that = this;
		$.each(deps, function(id, dep){
			if(dep.hasClass('washidden')){
				return;
			}
			// Set the values to their initial values before showing
			if (that.placeHolderQuestVals) {
				var updateQuestToPlaceHolder = function (q, placeholder) {
						q = $(q);
						if (q.find(':checkbox').length) {
							q.find(':checkbox').each(function(){ 
								$(this).prop('checked', placeholder.indexOf($(this).val()) > -1);
							})
						} else if (q.find(':radio').length) {
							q.find(':radio').prop('checked', false).filter('[value="'+placeholder+'"]').prop('checked', true);
						} else {
							q.find('.sg-input').val(placeholder);
						}
					};
				
				var plc = that.placeHolderQuestVals[id];
				if (dep.hasClass('sg-type-group')) {
					dep.find('.sg-question')
						.each(function(){
							var gplc = that.placeHolderQuestVals[this.id];
							if (gplc == null || gplc == undefined) return;
							updateQuestToPlaceHolder(this, gplc);
						})
				} else if (plc != null && plc != undefined && $(dep.selector.replace('-box','-element')).val() == '') {
					updateQuestToPlaceHolder(dep, plc);
				}
			}
			
			dep.show().removeClass('sg-hide'); 
			
			that.setMeta(dep, 'hidden', 'false');
			
			$("[checked='checked']",dep).removeAttr("checked").prop('checked',true);

			// if the dep is a group, show all questions inside
			if (dep.hasClass('sg-type-group')) {
				var subQuests = [];
				
				dep.find('.sg-question').each(function() {
					if($(this).parent().hasClass('sg-hide'))
						return;
					subQuests.push($(this));
				});
				
				that.showDependents(subQuests);
			}
		})
	},
	
	/**
	 * This function shows all the dependent questions passed to it.
	 * 
	 * @param {$, obj} deps
	 * 
	 */		
	showQuestion: function(qid) {
		var self = this;
		selector = '#sgE-'+ this.surveyObject.id +'-' +  this.pageId +'-'+ qid +'-box';

		if($(selector).hasClass('washidden')){
			return false;
		}
		// Set the values to their initial values before showing
		if (this.placeHolderQuestVals) {
			var updateQuestToPlaceHolder = function (q, placeholder) {
					q = $(q);
					if (q.find(':checkbox').length) {
						q.find(':checkbox').each(function(){ 
							$(this).prop('checked', placeholder.indexOf($(this).val()) > -1);
						})
					} else if (q.find(':radio').length) {
						q.find(':radio').prop('checked', false).filter('[value="'+placeholder+'"]').prop('checked', true);
					} else {
						q.find('.sg-input').val(placeholder);
					}
				};
			
			var plc = this.placeHolderQuestVals[qid];
			if ($(selector).hasClass('sg-type-group')) {
				$(selector).find('.sg-question')
					.each(function(){
						var gplc = self.placeHolderQuestVals[this.qid];
						if (gplc == null || gplc == undefined) return;
						updateQuestToPlaceHolder(this, gplc);
					})
			} else if (plc != null && plc != undefined && $($(selector).replace('-box','-element')).val() == '') {
				updateQuestToPlaceHolder($(selector), plc);
			}
		}
		
		$(selector).show().removeClass('sg-hide');
		$(selector).attr('data-shown', 'true');
		this.setMeta($(selector), 'hidden', 'false');
		
		$("[checked='checked']",$(selector)).removeAttr("checked").prop('checked',true);

		// if the dep is a group, show all questions inside
		if ($(selector).hasClass('sg-type-group') ) {
			var subQuests = [];
			
			$(selector).find('.sg-question').each(function() {
				if($(this).parent().hasClass('sg-hide'))
					return;

				var sku = $(this).attr('id').split('-')[3];
				if(sku > 0){
					//check subs for logic, eval or show if no logic
					var sub_quest = self.surveyObject.questions[sku];
					if(sub_quest){
						if(sub_quest.properties.complicated_logic_rule){
							eval(sub_quest.properties.complicated_logic_rule);
						}else{
							subQuests.push($(this));
						}
					}
				}else{
					$(this).attr('data-shown', 'true');
					subQuests.push($(this));
				}
			});
			this.showDependents(subQuests);
		}
		if ($(selector).hasClass('sg-type-table') ) {
			$(selector).show().removeClass('sg-hide');
			var subQuests = [];
			$(selector).find('tr').each(function() {
				if($(this).parent().hasClass('sg-hide'))
					return;
				//get subquestion id from row 	
				var row = $(this).find('.sg-first-cell').attr('id'),
				name = $('input' ,'#' + row).attr('name'),
				sku = 0,
				other = 0;

				//check for other row to ignore.
				if (name)
				{
					other =  other = name.indexOf("other");
				}

				if(row)
					sku = row.split('-')[1];
				if(sku > 0 && other  < 0){
					//check rows for logic, eval or show if no logic
					var sub_quest = self.surveyObject.questions[sku];
					if(sub_quest){
						if(sub_quest.properties.complicated_logic_rule){
							eval(sub_quest.properties.complicated_logic_rule);
						}else{
							subQuests.push($(this));
						}
					}
				}else{
					subQuests.push($(this));
				}
			});
			this.showDependents(subQuests);
		}
		if ($(selector).hasClass('sg-type-file') || $(selector).hasClass('sg-type-cascading-dropdown')){
			return
		}
		var new_selector = selector.replace('#','[name |= ').replace('-box',']')//fire change event on the selector so it would show all dependents as well
		if(!$(selector).hasClass('sg-type-maxdiff')){
			$(new_selector).change()
			$(new_selector).trigger('showhidden')
		}
		// Update mobile buttons in response to logic
		this.sgSurvey.adjustMobilePageControls();
	},
	
	/**
	 * This function hides all the dependent questions passed to it.
	 * It also clears the data for the hidden questions.
	 * 
	 * @param {obj} deps
	 * 
	 */	
	hideQuestion: function(qid) {
		selector = '#sgE-'+ this.surveyObject.id +'-' +  this.pageId +'-'+ qid +'-box';
		
		this.placeHolderQuestVals = this.placeHolderQuestVals || {};
		var that = this, 
			updatePlaceHolders = function (box) { // Remember the values of the questions so showDependents can repopulate if it needs to
				box = $(box);
				var id = box[0].qid;
				if(id != undefined){
					if (box.find(':checkbox').length) {
						that.placeHolderQuestVals[id] = [];
						box.find(':checkbox:checked').each(function(){ that.placeHolderQuestVals[id].push($(this).val()) })
						if (!that.placeHolderQuestVals[id].length) delete that.placeHolderQuestVals[id];
					} else if (box.find(':radio').length) {
						that.placeHolderQuestVals[id] = box.find(':radio:checked').val();
					} else {
						if (box.find('.sg-input').is('select') && box.find('.sg-input').val() == 'NoAnswer') return;
						that.placeHolderQuestVals[id] = box.find('.sg-input').val();
						if (that.placeHolderQuestVals[id] == '') delete that.placeHolderQuestVals[id];
					}
				}
			};

			$(selector).hide().addClass('sg-hide');
			
			this.setMeta($(selector), 'hidden', 'true');
			if (!!$(selector).find('.sg-input').length 
				&& this.placeHolderQuestVals[qid] == undefined
			) { // Remember default (placeholder) values
				if ($(selector).hasClass('sg-type-group')) {
					$(selector).find('.sg-question')
						.each(function(){
							updatePlaceHolders(this);
						})
				} else {
					updatePlaceHolders($(selector));
				}
			}
			
			// if the dep is a group, hide all questions inside
			if ($(selector).hasClass('sg-type-group')) {
				var subQuests = [];
				
				$(selector).find('.sg-question').each(function() {
					subQuests.push($(this));
				});
				
				this.hideDependents(subQuests);
			}

			if ($(selector).hasClass('sg-type-table')) {
				var subQuests = [];
				
				$(selector).find('tr').each(function() {
					if($(this).parent().hasClass('sg-hide'))
						return;
					subQuests.push($(this));
				});
				
				this.hideDependents(subQuests);
			}
		// Update mobile buttons in response to logic
		this.sgSurvey.adjustMobilePageControls();
	},
	
	showTableRow: function(sku, parent_sku) {
		selector = "tr.row-"+sku;
		$(selector).show().removeClass('sg-hide');
		this.setMeta($(selector), 'hidden', 'false');
		$(selector).attr('data-shown', 'true');

		var meta_selector = '#sgE-'+ this.surveyObject.id +'-' + this.pageId +'-'+ sku +'-meta';
		this.setMeta($(meta_selector), 'hidden', 'false');
		

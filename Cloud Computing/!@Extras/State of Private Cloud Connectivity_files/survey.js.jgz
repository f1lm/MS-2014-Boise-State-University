			priPos	= pos[0],
			secPos	= pos[1],
			dinkPos = pos[2] || this.oppositeDay(secPos);

		// set the primary position		
		switch (priPos) {

			case "left":	this.left -= this.bubWidth + padding; break;
			case "right":	this.left += this.elWidth + padding; break;
			case "top":		this.top -= this.bubHeight + padding; break;
			case "bottom":	this.top += this.elHeight + padding; break;
		}
		
		switch (priPos) {
			
			// for left and right bubbles, set the top and dink position
			case "left": case "right":					
		
				// set the dink adjustment and styles
				if (this.hasDink) {	
					dinkAdjust = setDink(dinkPos, (this.bubHeight / 10));
				}
				
				// calc the middle of element
				middle = (this.elHeight / 2);

				// if the dink offset is greater than middle, just use the middle
				if (dinkAdjust && dinkAdjust < middle) {

					switch (secPos) {
						case "top":		this.top += dinkAdjust; break;
						case "bottom":	this.top += this.elHeight - dinkAdjust; break;
					}
				}
				else {
					this.top += middle;
				}

				// adjust the top to account for the dink
				switch (dinkPos) {
					case "top":		this.top -= dinkAdjust; break;
					case "center":	this.top -= (this.bubHeight / 2); break;
					case "bottom":	this.top -= this.bubHeight - dinkAdjust; break;
				}
				
				break;
				
			// for top and bottom bubbles, set the left and dink position	
			case "top": case "bottom":					
		
				// set the dink adjustment and styles
				if (this.hasDink) {	
					dinkAdjust = setDink(dinkPos, (this.bubWidth / 10));
				}
				
				// calc the middle of element
				middle = (this.elWidth / 2);
				
				// if the dink offset is greater than middle, just use the middle
				if (dinkAdjust && dinkAdjust < middle) {

					switch (secPos) {
						case "left":	this.left += dinkAdjust; break;
						case "right":	this.left += this.elWidth - dinkAdjust; break;
					}
				}
				else {
					this.left += middle;
				}
				
				// adjust the left to account for the dink
				switch (dinkPos) {
					case "left":	this.left -= dinkAdjust; break;
					case "center":	this.left -= (this.bubWidth / 2); break;
					case "right":	this.left -= this.bubWidth - dinkAdjust; break;
				}	
				
				break;

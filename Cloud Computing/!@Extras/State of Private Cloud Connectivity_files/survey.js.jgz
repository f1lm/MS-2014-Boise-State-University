			}
			
			var len = a.length;
			
			if (len != b.length) { 
				return false; 
			}
			
			var aTest = a.sort();
			var bTest = b.sort();
				
			for (var i = 0; i < len; i++) {
				
				if (aTest[i] !== bTest[i]) { 
					return false;
				}
			}
			
			return true;
		},


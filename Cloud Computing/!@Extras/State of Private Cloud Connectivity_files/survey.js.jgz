		if (inLeft)		{ newPosition = newPosition.replace("left", "right") }
		if (inRight)	{ newPosition = newPosition.replace("right", "left") }
		if (inBottom)	{ newPosition = newPosition.replace("bottom", "top") }	
		
		// check if something changed...
		if (newPosition != this.position) {

			//...if so, run adjustment again
			this.adjustPosition(newPosition);
		}	
		
		// pull out the primary position from the new position
		var newPriPosition = newPosition.split("-")[0];
		
		// check if new primary position is different from last
		if (newPriPosition != this.priPosition) {
			
			// change class and reset position
			this.bubble.removeClass("bubble-" + this.priPosition);	
			this.bubble.addClass("bubble-" + newPriPosition);			
			this.priPosition = newPriPosition;			
		}	

	clearQuestionData: function( selector ) {
		var that = this;
		
		quest = $(selector).closest('.sg-question');
		
		if(selector.indexOf('row-') != -1)
			quest = $(selector).closest('tr');

		//don't clear data if the question was never shown (default values)
		if(!$(quest).attr('data-shown') 
			&& !quest.hasClass('sg-type-rank-dragdrop') 
			&& !quest.hasClass('sg-type-maxdiff')
			&& !quest.hasClass('sg-type-group')
			&& !quest.hasClass('sg-type-slider'))
			return;
			
		if(quest.hasClass('sg-type-group')){
			subs = quest.find('.sg-question');
			subs.each(function(){
				id = $(this).attr('id').replace('-box','');
				sel = '[name |= '+ id +']'

				if(!$(this).attr('data-shown') 
					&& !$(this).hasClass('sg-type-slider'))
					return;
					
				that.clearQuestionData(sel);
			})
		}
		
		if ((quest.hasClass('sg-type-menu') || quest.hasClass('sg-type-multimenu') || quest.hasClass('sg-type-table-menu-matrix')) && !quest.find(':input').hasClass('sg-input-menu-multiselect')) {
			quest.find('select').prop('selectedIndex', 0);
		}
		else if(quest.hasClass('sg-type-menu') && quest.find(':input').hasClass('sg-input-menu-multiselect')){
			quest.find('select option:selected').removeAttr("selected");
		}
		else if(quest.hasClass('sg-type-slider') || quest.hasClass('sg-type-multi-slider')){ //slider 
			var slider_selector = selector.replace('[name |= ','').replace(']','')
			quest.find('.sg-bubble-' + slider_selector).html('').css('min-height','12px')
			quest.find('.sg-slider-value').html('')
			quest.find('.ui-slider-handle').removeAttr('style');
			quest.find('input.sg-input[type="hidden"]').val('');
		}
		else if(quest.hasClass('sg-type-table-stars')){
			quest.find('input').removeAttr('checked');
			quest.find('.sg-star-0').mousedown() //click on reset
			quest.find('.sg-star-0 .sg-input-radio').prop('checked', 'checked')
			quest.find('input').change()
		}
		else if(quest.hasClass('sg-type-rank-table')){
			quest.find('input[type="radio"]').prop('checked', false);
		}
		else if (quest.hasClass('sg-type-rank-dragdrop')) {
			var attrs = ['id', 'name'];
			quest.find('ul[id$="-origin"], ul[id$="-target"]').each(function(){
				var $old = $SG(this).parent().siblings('.sg-sort-default');
				$SG(this).html($old.html());
				$SG(this).data().uiSortable && $SG(this).sortable( "refresh" );				
				for (var i = 0; i < attrs.length; i++) {
					$SG('[data-' + attrs[i] + ']', this).each(function(){
						$(this).attr(attrs[i], $(this).attr('data-' + attrs[i])).attr('data-' + attrs[i], '');
					});
				}
			});
			quest.find('input.sg-input[type="text"]').val('');
		}
		else if(quest.hasClass('sg-type-imageselect')){
			quest.find('.sg-imageselect-item').removeClass('sg-image-selected');
			quest.find('input').removeAttr('checked').change();
		}
		else if (quest.hasClass('sg-type-file')){
			quest.find('li:not(.sg-prototype-li) .sg-file-delete').click()
			return;
		}
		else if(quest.hasClass('sg-type-continuous-sum')){
			quest.find('input.sg-input[type="text"]').val('');
			quest.find('.sg-counter-sum').html('')
		}
		else if(quest.hasClass('sg-type-signature')){
		      quest.find('.sg-signature-clear').click();
		      quest.find('.sg-signature-name input').val('')
		}
		else {
			quest.find('select').prop('selectedIndex', 0);
			quest.find('input.sg-input').prop('checked', false);
			quest.find('input.sg-input[type="text"]').val('');
			quest.find('textarea.sg-input').val('');
			//individual row logic in star tables
			quest.find('input').removeAttr('checked');
			quest.find('.sg-star-0').mousedown() //click on reset
			quest.find('.sg-star-0 .sg-input-radio').prop('checked', 'checked')
		}
		
		$(selector).change()//fire change event to update all dependents
	},
	
	initLogic: function(question,logic,triggers){
		var self = this;
		var base = "#sgE-" + this.surveyObject.id + "-" +  this.pageId +'-'+ question.id + '-box';
		self.setupTriggers(triggers, logic, question.id)
		eval(logic)
	},
	
	setupTriggers: function(triggers, logic, id){
		var self = this
		, targetSku = id
		, targetSelect = "[id = sgE-" + this.surveyObject.id + "-" +  this.pageId +'-'+ targetSku +"-box]"
		$.each(triggers, function(key,sku){
			var quest = self.surveyObject.questions[sku];
			if(quest){
				var quest_type = quest.type
				if(quest_type == 'IMAGE_SELECT'){
					quest_type = quest.properties.image_select_type == 'single-select' ? 'IMAGE-SINGLE' : 'IMAGE-MULTI';
				}
				else if(quest_type == 'MENU'){
					quest_type = quest.properties.multiple == true ? 'MENU-MULTI' : 'MENU';
				}
			
				switch(quest_type) {
					case 'MENU':
					case 'RADIO':
					case 'IMAGE-SINGLE':
					case 'NPS':
						var selector = "[name = sgE-" + self.surveyObject.id + "-" +  self.pageId +'-'+ quest.id +"]"
						, prevState = false;
						$(selector).on('change', function(){
							prevState = $(targetSelect).is(':visible');
							eval(logic)
							self.updateNumbering(targetSelect, prevState)
						})
						break;
					case 'ESSAY':
					case 'TEXTBOX':
					case 'SLIDER':
					case 'HIDDEN':
						var timer
						var selector = "[name = sgE-" + self.surveyObject.id + "-" +  self.pageId +'-'+ quest.id +"]"
						, prevState = false;
						//keyup paste input blur
						$(selector).on('keyup paste input blur change', function(){
							timer && clearTimeout(timer);
							timer = setTimeout(function(){
								prevState = $(targetSelect).is(':visible');
								eval(logic)
								self.updateNumbering(targetSelect, prevState)
							}, 500);
						})
						break;
					case 'IMAGE-MULTI':
					case 'CHECKBOX':
					case 'MULTI_TEXTBOX':
						var selector = "[name |= sgE-" + self.surveyObject.id + "-" +  self.pageId +'-'+ quest.id +"]"
						, prevState = false;
						$(selector).on('change', function(){
							prevState = $(targetSelect).is(':visible');
							eval(logic)
							self.updateNumbering(targetSelect, prevState)
						})
						break;
					case 'MENU-MULTI':
						var selector = "#sgE-" + self.surveyObject.id + "-" +  self.pageId +'-'+ quest.id +"-element"
						, prevState = false;
						$(selector).on('change', function(){
							prevState = $(targetSelect).is(':visible');
							eval(logic)
							self.updateNumbering(targetSelect, prevState)
						})
						break;
					case 'CASCADING_DROPDOWN':
						var selector = "#sgE-" + self.surveyObject.id + "-" +  self.pageId +'-'+ quest.id +"-box select"
							, prevState = false, data = 1;
						$(selector).on('change', function(){
							prevState = $(targetSelect).is(':visible');
							eval(logic)
							self.updateNumbering(targetSelect, prevState)
						})
						break;
					default:
						return '';
						break;
				}
			}
		})
	},
	
	updateNumbering: function(selector, shown){
	
		var self = this;
		var selector = $(selector).closest('.sg-question');

		if(!$(selector).is(':visible') && shown){
			self.decrementNumbers($(selector));
		}
		if($(selector).is(':visible') && !shown){
			self.incrementNumbers($(selector));
		}

	},
	
	SG_evaluate: function(qid){
		var self = this
		, survey = this.surveyObject
		, sid 	 = this.surveyObject.id
		, pid	 = this.pageId;
		var quest = survey.questions[qid];
		if(quest){
			//is the question visible?
			var quest_selector = '#sgE-' + sid + '-' + pid + '-' + quest.id + '-box';
			if($(quest_selector).hasClass('sg-hide') && quest.type != 'HIDDEN'){
				return '';
			}
			
			var quest_type = quest.type
			if(quest_type == 'IMAGE_SELECT'){
				quest_type = quest.properties.image_select_type == 'single-select' ? 'IMAGE-SINGLE' : 'IMAGE-MULTI';
			}
			else if(quest_type == 'MENU'){
				quest_type = quest.properties.multiple == true ? 'MENU-MULTI' : 'MENU';
			}
			switch(quest_type) {
				case 'IMAGE-SINGLE':
				case 'NPS':
				case 'RADIO':
					var selector = "[name = sgE-" + sid + "-" +  pid +'-'+ quest.id +"]"
					return $(selector+':checked').val() || '';
					break;
				case 'ESSAY':
				case 'TEXTBOX':
				case 'HIDDEN':
					var selector = "[name = sgE-" + sid + "-" +  pid +'-'+ quest.id +"]"
					if($.isNumeric($(selector).val())){
						return $(selector).val() * 1
					}
					else{
						return $(selector).val() || '';
					}
					break;
				case 'IMAGE-MULTI':
				case 'CHECKBOX':
				case 'MULTI_TEXTBOX':
					var selector = "[name |= sgE-" + sid + "-" +  pid +'-'+ quest.id +"]"
					var allSelected = []
					$(selector+':checked').each(function() {
						//check value for star ranking options
						if($(this).val().length)
							allSelected.push($(this).val());
					});
					return allSelected.length ? allSelected : '';
					break;
				case 'MENU':
					var selector = "[name = sgE-" + sid + "-" +  pid +'-'+ quest.id +"]"
					selected = $(selector).val() != 'NoAnswer' ? $(selector).val() : '';
					return selected;
					break;
				case 'MENU-MULTI':
					var selector = "#sgE-" + sid + "-" +  pid +'-'+ quest.id +"-element"
					var allSelected = []
					$(selector +" option:selected" ).each(function() {
						allSelected.push($(this).val());
					});
					return allSelected.length ? allSelected : '';
					break;
				case 'SLIDER':
					var selector = "[name = sgE-" + sid + "-" +  pid +'-'+ quest.id +"]"
					return $(selector).val() * 1 || '';
					break;
				case 'CASCADING_DROPDOWN':
					var selected = [];
					$SG(quest_selector + ' select').each(function(){
						var $opt = $SG('option:selected', this);
						if ($opt.length && $opt.val() != 'NoAnswer' && $opt.val() != '')
							 selected.push($opt.val());
					});
					return selected.length ? selected : '';
					break;
				default:
					return '';
					break;
			}
		}		
	},
	/**
	 * Set up the show / hide dependencies for a question.
	 * 
	 * @param {obj} question
	 */
	initShowHideTriggers: function(question) {
		
		var self = this;
		var base = "#sgE-" + this.surveyObject.id + "-" +  this.pageId +'-'+ question.id;

		this.optionDepsMap[question.id] = {};
		
		if (question.properties.hide_all_after_until_value == true) {
			this.surveyObject.hide_all_after = question.id;
		}	
		
		//add the handler to show the others when it's clicked... fork on input type
		if (question.type == 'RADIO' || question.type == 'CHECKBOX' || question.type == 'MENU' || question.type == 'NPS') {
						
			var dep, optId, optDeps, optDepsArr, optEl;
			var len = 0;
			
			function makeDomId(id) {
				return ['#sgE-', self.surveyObject.id, '-', self.pageId, '-', id, '-box'].join('');
			}
			
			// add listeners for menus
			if (question.type == 'MENU') {

				var triggerEl = $(base + '-element');
				
				triggerEl.bind('change', function(e) {
					self.checkQuestionDependents(e);
				});
			}
			
			for (var o in question.options) {
				
				optId = base + '-' + question.options[o].id;
				
				// add listeners for radios and checkboxes
				if (question.type != 'MENU') {
					
					// this is the option element
					optEl = $(optId);
				
					optEl.bind('click', function(e) {
						self.checkQuestionDependents(e);
					});		
					
					$(optId + '-other').bind('click', {el:optEl}, function(ev) {
						ev.data.el.prop('checked', true);
						ev.data.el.trigger('click');
					});
				}
				
				// skip options without dependents
				if (!question.options[o].properties || !question.options[o].properties.dependent) {					
					continue;
				}
				optDeps = question.options[o].properties.dependent;
				optDeps = optDeps + '';
				optDeps = optDeps.split(/,/);
				optDepsArr = [];				
				len = optDeps.length;
				
				// loop through option specific dependents
				for (var i = 0; i < len; i++) {				
					
					dep = $(makeDomId(optDeps[i]));
					
					optDepsArr.push(dep);
				}
				
				// save opt/deps info to global list
				this.optionDepsMap[question.id][question.options[o].id] = {
					el: (question.type == 'MENU') ? triggerEl : optEl,
					deps: optDepsArr
				}
			}
		}
	},
	
	/**
	 * This function is the brains behind show/hide triggers. Everytime 
	 * it runs, it checks the status of the page and hides/shows the correct 
	 * questions. It uses recursion to check the status again everytime something
	 * changes.  
	 * 
	 * @param {array} testArr
	 */
	checkQuestionDependents: function(testArr) {
		var click = false;
		for (var i = 0, j = arguments.length; i < j; i++){
			if (arguments[i] && arguments[i].originalEvent) click = true;
		}
		var optMap, opt, el, optDep, depId, optionArr, isSelected;
		var hideList = {};
		var showList = {};
		var hideArr = [];
		var self = this;
		
		// loop through every question with dependents
		for (var qid in this.optionDepsMap) {
			optMap = this.optionDepsMap[qid];

			// loop through the options for this question
			for (var oid in optMap) {
				opt = optMap[oid];
				el = opt.el;
				optionArr = opt.deps;
				
				// loop through array of dependents for this option
				for (var i = 0; i < optionArr.length; i++) {
					optDep = optionArr[i];
					depId = optDep.attr('id');

					// whether the input is selected (for menus or checkbox/radio)
					if (el.is('select')) {
						if ($.isArray(el.val())) {
							isSelected = $.inArray(oid, el.val()) != -1;
						} else {
							isSelected = el.val() == oid;
						}
					} else {
						isSelected = el.is(':checked');
					}
						
					if (isSelected) {
						showList[depId] = optDep;
					}
					else {
						hideList[depId] = optDep;
						hideArr.push(depId);
					}	
				}
			}
		}

		// remove any shows in the hide list
		for (var x in hideList) {
			if (showList[x]) {
				delete hideList[x];
			}
		}
		
		if(self.dNumbering){
			for (var x in hideList){
				if($('#' + x).is(':visible')){
					self.decrementNumbers($('#' + x));
				}
			}
			for (var x in showList){
				if(!$('#' + x).is(':visible')){
					self.incrementNumbers($('#' + x));
				}
			}
		}

		// if something has changed from the last time, run again
		if (!SGAPI.array.compare(testArr, hideArr)) {
			this.hideDependents(hideList, click);
			this.showDependents(showList);
			this.checkQuestionDependents.apply(this, click ? [hideArr, {originalEvent : true}] : [hideArr]);
		}
	},


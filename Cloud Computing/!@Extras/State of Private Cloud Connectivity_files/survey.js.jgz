	var CascadingDropdown = function(params)
	{
		params = params || {};

		this.base = params.base;
		this.container = $(params.base + '-box');
		this.questionID = params.qid;
		this.optionsCache = {};

		this.init();
	};
	
	CascadingDropdown.prototype.init = function()
	{
		var self = this;
		var count = 0;

		this.container.find('select').each(function() {
			self.init_dropdown($(this), count);
			count++;
		});

	};
	
	CascadingDropdown.prototype.init_dropdown = function(el, order_id)
	{
		var self = this;

		el.attr('data-order_id', order_id);

		// first drop down always shows
		if (order_id == 0) {
			this.getOptions(el, []);
			el.parent().parent().show();
		}

		el.change(function() {

			var order_id = parseInt(el.attr('data-order_id'));
			var selected = [];
			var newVal = el.val();

			self.container.find('select').each(function() {

				var currentSelect = $(this);

				var current_order_id = parseInt(currentSelect.attr('data-order_id'));

				if (current_order_id > order_id) {

					// all selects after the current get data cleared and options cleared
					currentSelect.val(''); // clear the value
					currentSelect.html('<option value=""> -- ' + SGAPI.util.translate('Please Select') + ' -- </option>'); // clear the options

					if (current_order_id > order_id + 1 || newVal == '') {

						// only the drop down immediately following the one that changed should show at all,
						currentSelect.parent().parent().hide();

					} else {

						// this select is the next one the user might select, so it needs to show
						currentSelect.parent().parent().show();

						// we will need to update the options in this drop down, as they may have changed
						self.getOptions(currentSelect, selected);

					}

				} else {

					// this one needs to remain showing
					currentSelect.parent().parent().show();

					selected.push(currentSelect.val());

				}

			});

			return null;
		});
	};

	CascadingDropdown.prototype.getOptions = function(selectedEl, selected) 
	{
		var self = this,
			id   = selectedEl.prop('id') +'-'+ window.encodeURIComponent(selected);
			
		selectedEl.prop('disabled', true);
		selectedEl.html('<option value=""> ' + SGAPI.util.translate('Loading...') + ' </option>');

		var setOptions = function(data) {

			var vals = data.values || [];
			var markup = [];

			markup.push('<option value=""> -- ' + SGAPI.util.translate('Please Select') + ' -- </option>');

			for(var i = 0; i < vals.length; i++){
				markup.push('<option value="' + vals[i] + '">');
				markup.push(vals[i]);
				markup.push('</option>');
			}

			selectedEl.html(markup.join(''));

			selectedEl.prop('disabled', false);

			// see if this select has data from previous interaction, if so, pre-populate
			var existing = selectedEl.attr('data-previous_value');
			if (existing !== undefined) {
				selectedEl.val(existing);
				setTimeout(function(){
					selectedEl.trigger('change');
				}, 0);
				selectedEl.removeAttr('data-previous_value'); // no longer needed
			}
		};
		
		if (this.optionsCache[id]) {
			setOptions(this.optionsCache[id]);
		} else {
			$.ajax({
				url : '/s3/ajax_cascadingdropdown.php',
				data: {
					'id' 						  : SGAPI.survey.surveyObject.id,
					'question_sku' 				  : this.questionID,
					'selected' 					  : selected,
					'cascading-dropdown-language' : encodeURIComponent(SGAPI.survey.surveyObject.language)
				},
				cache: false,
				dataType : 'json',
				success : function(data) {
					// update cache
					self.optionsCache[id] = data;

					// handle options
					setOptions(self.optionsCache[id]);
				}
			});
		}

	};
	
	slider = function(optionid,orientation,min,max,stepval,startval,single,showval,flipped,sliderval)
	{
		var flipped = flipped;
		var optionid = optionid;
		var orientation = orientation;
		var min = min;
		var max = max;
		var stepval = stepval;
		var single = single;
		var showval = showval;
		var sliderval = sliderval;
		var startval = flipped === false ? startval : (max - startval + min);
		
		var dink = $('<div class="sg-dink"><div></div></div>');
		var bubble = $('<div class="sg-slider-bubble-'+orientation+'"><div class="sg-bubble-'+optionid+'"></div></div>');
		dink.appendTo(bubble);
		$(".slider-"+optionid).slider({
				create: function(event, ui){
					var hiddenval = $(this).siblings('.sg-slider-hidden-value').find('.sg-input-hidden').val();
					if(sliderval){
						if(flipped === false){
							$(this).slider('value', sliderval);
						}
						else{
							$(this).slider('value', (max - sliderval));
						}
						$('.sg-slider-value-'+optionid).html(sliderval);
						if(single == true && showval == true){						
							bubble.appendTo('.slider-'+optionid+' .ui-slider-handle');
							$('.sg-bubble-'+optionid).text(sliderval);
						}
					}
					else{
						if(hiddenval){
							$(this).slider('value', hiddenval);
							$('.sg-slider-value-'+optionid).html(hiddenval);
							if(single == true && showval == true){
								$('.sg-bubble-'+optionid).text(hiddenval);
								bubble.appendTo('.slider-'+optionid+' .ui-slider-handle');
								$('.sg-bubble-'+optionid).text(hiddenval);
							}
						}
						else{
							$(this).slider('value', startval);
							$('.sg-slider-value-'+optionid).html();
						}
					}
				},
				animate: true,
				min: min,
				max: max,
				step: stepval,
				orientation: orientation,
				slide: function( event, ui ) {
					if(flipped === false){
						$(this).siblings('.sg-slider-hidden-value').find('.sg-input-hidden').val(ui.value).change();
						$('.sg-slider-value-'+optionid).text(ui.value);
						if(single == true && showval == true){
							$('.sg-bubble-'+optionid).text(ui.value);
						}
					}
					else{
						$(this).siblings('.sg-slider-hidden-value').find('.sg-input-hidden').val((max - ui.value) + min).change();
						$('.sg-slider-value-'+optionid).text((max - ui.value)  + min);
						if(single == true && showval == true){
							$('.sg-bubble-'+optionid).text((max - ui.value) + min);
						}
					}
				},
				stop: function () {
					$(this).siblings('.sg-slider-hidden-value').find('.sg-input-hidden').trigger('sgquestchange');
				},
				start: function(event, ui) {
					$(this).removeClass('ui-slider-'+orientation+'-blank');
					if(single == true && showval == true && $(this).siblings('.sg-slider-hidden-value').find('.sg-input-hidden').val() == ''){
						if(flipped === false){
							bubble.find('.sg-bubble-'+optionid).text(ui.value);
						}
						else{
							bubble.find('.sg-bubble-'+optionid).text((max - ui.value) + min);
						}
						bubble.appendTo('.slider-'+optionid+' .ui-slider-handle');
					}
					if(flipped === false){
						$(this).siblings('.sg-slider-hidden-value').find('.sg-input-hidden').val(ui.value);
						$('.sg-slider-value-'+optionid).html(ui.value);
					}
					else{
						$(this).siblings('.sg-slider-hidden-value').find('.sg-input-hidden').val((max - ui.value) + min);
						$('.sg-slider-value-'+optionid).html((max - ui.value) + min);
					}
				}
			});		
			$("#amount" ).val( "$" + $("#slider").slider("value"));
	}
	


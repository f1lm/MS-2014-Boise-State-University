	hideTableRow: function(sku, parent_sku) {
		selector = "tr.row-"+sku;
		$(selector).hide().addClass('sg-hide');
		this.setMeta($(selector), 'hidden', 'true');

		var parent = '#sgE-'+ this.surveyObject.id +'-' +  this.pageId +'-'+ parent_sku +'-box';
		var rows = $(parent).find("tr:not(.sg-hide)");
		if(!$(parent).hasClass("sg-hide")){

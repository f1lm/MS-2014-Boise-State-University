/**
* This is a jquery extention that creates "bubbles"
* on a specified group of elements.
*
* @author Pete Fredricks - 02/15/2011
*
**/
(function($){

	// ********** START BUBBLE CLASS ********** //	
		
	// Bubble constructor. This is the base class.
	var Bubble = function() {
		this.trigger = "hover-hover"
		this.position = "bottom-right"
	};

	/**
	 * This inits the class once it is instantiated.
	 *
	 * @param {jQuery obj} el
	 * @param {obj} options
	 */
	Bubble.prototype.init = function(el, options) {
		
		options = options || {};
		
		// all these "if" statements override the default (whether or not there is one).
		if (options.dink)		{ this.hasDink	= (options.dink  == "true") }
		if (options.auto)		{ this.autoPos	= (options.auto  == "true") }
		if (options.animate)	{ this.animate	= (options.animate  == "true") }
		if (options.mouse)		{ this.toMouse	= (options.mouse  == "true") }
		if (options.fixed)		{ this.fixedPos	= (options.fixed  == "true") }
		
		if (options.trigger)	{ this.trigger	= options.trigger }
		if (options.position)	{ this.position	= options.position }		
			
		if (options.width)		{ this.width	= parseInt(options.width) }
		if (options.height)		{ this.height	= parseInt(options.height) }
		if (options.top)		{ this.top		= parseInt(options.top) }
		if (options.left)		{ this.left		= parseInt(options.left) }
		
		this.showDelay = options.showdelay ? parseInt(options.showdelay) : 0;
		this.hideDelay = options.hidedelay ? parseInt(options.hidedelay) : 400;
		
		this.debug = (options.debug == "true");
		this.priPosition = this.position.split("-")[0];
		this.bubblePadding = parseInt(options.padding) || ((this.toMouse) ? 20 : 5);

		this.bubble = null;
		this.dink = null;
		this.message = options.msg;
		this.contentId = options.id;
		this.uid = SGAPI.util.getUID();
		
		// keep these as arrays so it's easy to add things.
		this.innerClass = ["bubble-inner"];	
		this.bubbleClass = ["bubble ", (options.type || "basic"), "-bubble"];
		
		if (options.style) {
			this.innerClass.push(" ", options.style);
		}
		
		// these properties only work with Template Manager
		if (SGAPI.TemplateManager && options.template) {
			this.template = options.template;		
			this.dataKey = options.datakey;
			this.templateFile = (options.templateroot || "/public/templates/") + this.template + ".tmpl";
		}
		
		// the orignal position of target
		this.origTop = null;
		this.origLeft = null;

		this.el = el;
		this.elWidth = 0;
		this.elHeight = 0;
		
		this.timer = null;
		this.status = false;
		
		this.runSetup();
		
		// set uid 
		if (this.bubble) {			
			this.bubble.data('bubble.uid', this.uid);
			this.el.data('bubble.uid', this.uid);
		}
	}

	/**
	 * This function will probably be overwritten but the child classes. It is always 
	 * run at the end of the init, so it's up to the child class what goes in there.
	 */	
	Bubble.prototype.runSetup = function() {
		
		// set the content - this returns false if content is coming from template
		this.loadContent({
			callback: this.draw,
			context: this
		});
	}
	
	/**
	 * Remove Bubble from the DOM
	 */	
	Bubble.prototype.destroy = function() {		
		this.bubble.remove();
	}
	
	/**
	 * This function gets the content of the bubble. It can either be a message 
	 * passed in the options, using a template, or it is taken from a specified
	 * container (using the id).
	 * 
	 * @param {obj} oArgs
	 */
	Bubble.prototype.loadContent = function(oArgs) {

		oArgs = oArgs || {};
		
		// if there is template specified, run the templating code.
		if (this.template) {
			
			var tempArgs = { 
				paths: this.templateFile,
				onload: templateLoaded,
				context: this
			}

			$.loadTemplates(tempArgs);
		}
		else { 
			
			// non-template based content...
			
			// if there is a message aready passed, just use that.
			if (this.message) {
				this.content = this.message;
			}

			// if there is a contentId, use that element
			if (this.contentId) {
				this.content = $("#" + this.contentId);
				
				if (this.content[0]) {
					this.content[0].style.display = '';
				}
			}
			
			// if the content is an iframe, set the frame src
			if (this.frame) {
				this.frame.attr('src', this.el.attr("bubble-src"));
			}
			
			// pull content from the title 
			if (!this.content && this.el.attr("title")) {
				this.content = this.el.attr("title");
				this.el.removeAttr("title");
			}		
			
			// run callback if we have one	
			if (oArgs.callback) {
				oArgs.callback.call(oArgs.context, oArgs.data);
			}
		}
		
		// get data if there is a datakey and run filltemplate when done.
		function templateLoaded() {

			if (this.dataKey) {

				var key = this.dataKey.split("-");
				var data = {datakey: key[1]}
				var url = key[0];

				$.ajax({
					url: url,
					dataType: 'json',
					data: data,
					success: fillTemplate,
					context: this
				});
			}
			else {
				fillTemplate.call(this, {});
			}
		}

		// set content and run callback.
		function fillTemplate(result) {
			this.content = $.fillTemplate(this.template, result);

			if (oArgs.callback) {
				oArgs.callback.call(oArgs.context, oArgs.data);
			}
		}
	}

	/**
	 * This function draws the actual bubble. It is responsible for adding the inner div,
	 * the inner div classes, the content, as well as binding events and setting position.
	 */
	Bubble.prototype.draw = function() {

		// create the inner div, add any classes, and add the content
		var inner = this.inner = $('<div></div>')
			.addClass(this.innerClass.join(""))
			.html(this.content);
			
		if (this.width) {
			inner.css({
				width: this.width,
				'max-width': this.width
			});
		}

		if (this.height) {
			inner.height(this.height);
		}
		
		// add the position to the bubble class
		this.bubbleClass.push(" bubble-", this.priPosition);

		// create bubble, add the inner div to the bubble, and attach the whole bubble to body
		this.bubble = $('<div style="top:-5000px; left:-5000px;"></div>')
			.append(inner)
			.appendTo($('body'))
			.attr("class", this.bubbleClass.join(""));			
		
		// add the dink to bubble, if necessary
		if (this.hasDink) {
			this.dink = $('<div class="bubble-dink"><div></div></div>');
			this.bubble.prepend(this.dink);
		}
		
		// bind the events to the bubble and handle
		this.bindEvents();
	}

	/**
	 * This function ultimately sets the position and class of the bubble.
	 *
	 * @param {event obj} ev
	 */
	Bubble.prototype.setPosition = function(ev) {
		
		if (!this.fixedPos) {

			// reset height and width to total bubble
			this.bubWidth = this.bubble.outerWidth(true);
			this.bubHeight = this.bubble.outerHeight(true);

			// if "toMouse" then orignal position is based on mouse postion.
			// otherwise it's based on target element position.
			if (this.toMouse && ev) {

				this.origTop = ev.pageY;
				this.origLeft = ev.pageX;
			}
			else {				
				// get the target element's height and width
				this.elWidth = this.el.outerWidth(true);
				this.elHeight = this.el.outerHeight(true);
				
				// store anchor element's offset
				var elOffset = this.el.offset();

				this.origTop = elOffset.top;
				this.origLeft = elOffset.left;
			}			
			
			this.adjustPosition(this.position);

			// only run if auto-position is turned on
			if (this.autoPos) {
				this.autoPosition();
			}
		}

		// set the bubble position
		this.bubble.css({
			top: this.top,
			left: this.left,
			display: "none"
		});
	}

	/**
	 * This function takes the string version of position and creates
	 * the actual coordinates.
	 *
	 * @param {string} position
	 */
	Bubble.prototype.adjustPosition = function(position) {

